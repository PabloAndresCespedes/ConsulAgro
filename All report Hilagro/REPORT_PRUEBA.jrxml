	<queryString>
		<![CDATA[SELECT 1 ITEM,
       'LUNES - SEMANAL' DIA,
       D.FCAT_DESC,
       A.CLI_CODIGO,
       SUBSTR(A.CLI_NOM, 1, 35) CLI_NOM,
       SUBSTR(E.ZONA_DESC, 1, 20) ZONA_DESC,
       SUBSTR(A.CLI_DIR, 1, 30) CLI_DIR,
       SUBSTR(A.CLI_TEL, 1, 15) CLI_TEL,
       SUBSTR(CLI_PERS_CONTACTO, 1, 30) CLI_CONTACTO,
       CLI_VENDEDOR,
       A.CLI_ORDEN_LUN ORDEN,

       F.FCAT_CODIGO,
       F.FCAT_DESC,
       G.RAMO_CODIGO,
       G.RAMO_DESC,
       H.CAN_CODIGO,
       H.CAN_DESC,
       FH.HOL_OBS_FACT ,
       DECODE(A.CLI_IND_EXHIBIDOR, 'S', 'S', NULL) CLI_IND_EXHIBIDOR,
       A.CLI_NOM_FANTASIA,
       NVL(A.CLI_FREC_VISITA_SEMANAL, 'N') SEMANAL,
       NVL(A.CLI_FREC_VISITA_QUINCENAL, 'N') QUINCENAL

  FROM FIN_CLIENTE     A,
       FAC_CATEGORIA   D,
       FAC_ZONA        E,
       FAC_CATEGORIA   F,
       FIN_RAMO        G,
       FAC_CANAL_VENTA H,
       FIN_FICHA_HOLDING FH
 WHERE A.CLI_CATEG = D.FCAT_CODIGO
   AND  A.CLI_COD_FICHA_HOLDING = FH.HOL_CODIGO
   AND CLI_EMPR = D.FCAT_EMPR
   AND CLI_EMPR = CAN_EMPR
   AND CLI_EMPR = ZONA_EMPR
   AND CLI_EMPR =  $P!{P_EMPRESA}
   AND CLI_EMPR = RAMO_EMPR
   AND CLI_EMPR = F.FCAT_EMPR
   AND A.CLI_ZONA = E.ZONA_CODIGO
   AND NVL(A.CLI_DIA_LUNES, 'N') = 'S'

   AND A.CLI_CATEG = F.FCAT_CODIGO
   AND A.CLI_RAMO = G.RAMO_CODIGO
   AND A.CLI_CANAL = H.CAN_CODIGO
   AND A.CLI_FREC_VISITA_SEMANAL = 'S'
    AND CLI_EMPR =  FH.HOL_EMPR
   AND CLI_EST_CLI = 'A'
       AND (CLI_CODIGO  = $P{P_CLI_CODIGO} or  $P{P_CLI_CODIGO} is null)
          AND (CLI_VENDEDOR  = $P{P_CLI_VENDEDOR} or  $P{P_CLI_VENDEDOR} is null)
       AND (F.FCAT_CODIGO  = $P{P_FCAT_CODIGO} or  $P{P_FCAT_CODIGO} is null)
      AND (CLI_SUCURSAL = $P{P_CLI_SUCURSAL} or  $P{P_CLI_SUCURSAL} is null)

