	<parameter name="P_IND_COPIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT A.DOC_FEC_DOC FECHA,
       A.DOC_CLI_NOM CLIENTE,
       A.DOC_CLI_RUC RUC,
       D.CLI_DIR,
       A.DOC_MON,
       D.CLI_TEL,
       A.DOC_NRO_DOC NRO_DOC,
       NVL(DCON_CANT_AUX, 1) CANT,
       DECODE(B.DCON_OBS, NULL, C.FCON_DESC, DCON_OBS) ARTICULO_DES,
       ((B.DCON_EXEN_MON + B.DCON_GRAV_MON) +
       ROUND((B.DCON_GRAV_MON * B.DCON_PORC_IVA / 100), 2)) /
       NVL(DCON_CANT_AUX, 1) PRECIO_UN,
       DECODE(NVL(DCON_PORC_IVA, 0), 0, A.DOC_NETO_EXEN_MON, 0) EXENTO,
       DECODE(NVL(DCON_PORC_IVA, 0),
              5,
              (B.DCON_EXEN_MON + B.DCON_GRAV_MON) +
              ROUND(((B.DCON_EXEN_MON + B.DCON_GRAV_MON) * B.DCON_PORC_IVA) / 100,
                    2),
              0) NETO_5,
       DECODE(NVL(DCON_PORC_IVA, 0),
              10,
              (B.DCON_EXEN_MON + B.DCON_GRAV_MON) +
              ROUND(((B.DCON_EXEN_MON + B.DCON_GRAV_MON) * B.DCON_PORC_IVA) / 100,
                    2),
              0) NETO_10,
       (DECODE(NVL(DCON_PORC_IVA, 0), 0, A.DOC_NETO_EXEN_MON, 0) +
       DECODE(NVL(DCON_PORC_IVA, 0),
               5,
               (B.DCON_EXEN_MON + B.DCON_GRAV_MON) +
               (((B.DCON_EXEN_MON + B.DCON_GRAV_MON) * B.DCON_PORC_IVA) / 100),
               0) +
       DECODE(NVL(DCON_PORC_IVA, 0),
               10,
               (B.DCON_EXEN_MON + B.DCON_GRAV_MON) +
               (((B.DCON_EXEN_MON + B.DCON_GRAV_MON) * B.DCON_PORC_IVA) / 100),
               0)) TOTAL,
       A.DOC_IVA_5_MON SUMA_IVA_5,
       A.DOC_IVA_10_MON SUMA_IVA_10,

       'TIMBRADO NÂº ' || TIMB_NUMERO TIMB_NUMERO,
       'FECHA VIG. ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_INIC, 'MONTH')) || ' ' ||
       TO_CHAR(TIMB_FECH_INIC, 'YYYY') FEC_TIM_INI,
       'FECHA FIN ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH')) || ' ' ||
       TO_CHAR(TIMB_FECH_FIN, 'YYYY') FEC_TIM_FIN,
       TIMB_AUTORIZACION,
       TIMB_NUMERO,
       TIMB_IMPRESORA,
       MON_DEC_IMP,
       DOC_NRO_NC,
       MON_CODIGO,
       MON_DESC,
       DECODE(B.DCON_OBS, NULL, C.FCON_DESC, DCON_OBS) ARTICULO,
       A.DOC_IVA_5_MON+ A.DOC_IVA_10_MON TOTAL_IVA,
       DOC_CLAVE,
       NULL ART_CODIGO,
       NULL FAC_A_APLICAR
  FROM FIN_DOC_CONCEPTO B,
       FIN_CONCEPTO     C,
       FIN_DOCUMENTO    A,
       FIN_CLIENTE      D,
       GEN_TIPO_MOV     H,
       FIN_TIMBRADO     J,
       GEN_MONEDA       M
 WHERE B.DCON_EMPR = $P{P_EMPRESA}

   AND B.DCON_CLAVE_DOC = A.DOC_CLAVE
   AND B.DCON_EMPR = A.DOC_EMPR

   AND B.DCON_CLAVE_CONCEPTO = C.FCON_CLAVE
   AND B.DCON_EMPR = C.FCON_EMPR

   AND A.DOC_CLI = D.CLI_CODIGO
   AND A.DOC_EMPR = D.CLI_EMPR

   AND A.DOC_TIPO_MOV = 16
   AND DOC_MON = MON_CODIGO
   AND DOC_EMPR = MON_EMPR

   AND H.TMOV_CODIGO = DOC_TIPO_MOV
   AND H.TMOV_EMPR = DOC_EMPR

   AND H.TMOV_CLASE = J.TIMB_CLASE_MOV
   AND H.TMOV_EMPR = J.TIMB_EMPR

   AND nvl(A.DOC_NRO_TIMBRADO,a.doc_timbrado) = J.TIMB_NUMERO
   AND A.DOC_EMPR = J.TIMB_EMPR

   AND A.DOC_NRO_DOC BETWEEN TIMB_DOCU_INIC AND TIMB_DOCU_FIN
   AND B.DCON_IVA_LOC = 0
   AND DOC_CLAVE =  $P!{P_CLAVE}]]>
	</queryString>
