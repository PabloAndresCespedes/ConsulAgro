	<subDataset name="logo" uuid="3aa76401-5301-49ad-9388-cd5ac8c3818f">
		<parameter name="P_EMPRESA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select EMPR_LOGO logo
from gen_empresa

where EMPR_CODIGO = $P{P_EMPRESA}]]>
		</queryString>
		<field name="LOGO" class="java.io.InputStream"/>
	</subDataset>
	<parameter name="P_EMPRESA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_ORDEN_DECRETO" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_OBSERVACION" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT S.ORD_EMPLEADO,
       S.ORD_CI,
       S.ORD_RUC,
       S.ORD_ORIGEN,
       S.ORD_DESTINO,
       S.ORD_HORARIO_DESDE,
       S.ORD_HORARIO_HASTA,
       S.ORD_DECRETO,
       S.ORD_ENCARGADO,
       S.ORD_ENCARGADO_CI ,
       D.DEC_NUMERO ,
       D.DEC_ARTICULO ,
       D.DEC_ART_DESC ,
       D.DEC_DETALLE ,
       TO_CHAR(D.DEC_FECHA_DESDE ,'DD/MM/YYYY') FECHA_DESDE ,
       TO_CHAR(D.DEC_FECHA_HASTA ,'DD/MM/YYYY') FECHA_HASTA ,
       (TO_CHAR(SYSDATE ,'MONTH')) MES ,
      ( select to_char(sysdate, 'Month','nls_date_language=spanish') as mes from dual ) MES2 ,

       (TO_CHAR(SYSDATE ,'DD')) DIA ,
       (TO_CHAR(SYSDATE ,'YYYY'))ANHO ,
      (SELECT  P.EMPL_NOMBRE||' '||P.EMPL_APE   FROM PER_EMPLEADO P
       WHERE P.EMPL_LEGAJO = S.ORD_EMPLEADO
          AND P.EMPL_EMPRESA = $P{P_EMPRESA}) EMPLEADO

  FROM FIN_ORDEN_SERVICIO S , FIN_DECRETO D
  WHERE D.DEC_CODIGO = S.ORD_DECRETO
  AND S.ORD_CODIGO = $P{P_ORDEN_DECRETO}
  AND S.ORD_EMPR = $P{P_EMPRESA}]]>
	</queryString>
