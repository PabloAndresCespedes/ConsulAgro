		<queryString>
			<![CDATA[SELECT UPPER(MOAN_CODIGO||' - '||MOAN_DESC) A, $P{P_NOMBRE} B
  FROM GEN_MOTIVO_ANULACION
 WHERE MOAN_EMPR = $P{P_EMPRESA}
   AND MOAN_AFEC_OCARGA = 'S'
    and MOAN_CODIGO is not null
 ORDER BY MOAN_CODIGO]]>
