	<subDataset name="RESUMEN_MARCA" uuid="7c790568-40c0-41d7-b166-ccff1db0e3fe">
		<parameter name="P_EMPRESA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_PERIODO" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_MARCA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_LINEA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_GRUPO" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_existencia" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_SESSION_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT ART_MARCA||'-'||MARC_DESC Marca_Resumen,
       Sum(AEP_TOT_EXIST) Marca_Cantidad,
       sum(AEP_TOT_EXIST * AEP_COSTO_PROM_LOC) Marca_TOT_LOC,
       sum(AEP_TOT_EXIST * AEP_COSTO_PROM_MON) Marca_TOT_MON
  FROM STK_MARCA,
       STK_LINEA,
       STK_GRUPO,
       STK_ARTICULO,
       GEN_EMPRESA,
       STK_ART_EMPR_PERI
 WHERE ART_CODIGO = AEP_ART
   AND ART_EMPR = AEP_EMPR
   AND MARC_CODIGO = ART_MARCA
   AND MARC_EMPR = ART_EMPR
   AND LIN_CODIGO = ART_LINEA
   AND LIN_EMPR = ART_EMPR
   AND GRUP_LINEA = ART_LINEA
   AND GRUP_EMPR = ART_EMPR
   AND GRUP_CODIGO = ART_GRUPO
   AND EMPR_CODIGO = AEP_EMPR
   AND EMPR_CODIGO = $P!{P_EMPRESA}
   AND AEP_PERIODO = $P!{P_PERIODO}
 and art_tipo<>4
   AND EMPR_CODIGO =$P{P_EMPRESA}
   AND EMPR_CODIGO = AEP_EMPR
   AND AEP_PERIODO =$P{P_PERIODO}
  AND (MARC_CODIGO =$P{P_MARCA} or $P{P_MARCA} is  null)
  AND (ART_LINEA =$P{P_LINEA} or $P{P_LINEA} is null)
  AND (ART_GRUPO =$P{P_GRUPO} or $P{P_GRUPO} is null)
  AND (MARC_CODIGO =$P{P_MARCA} or $P{P_MARCA} is null)
  AND (ART_LINEA =$P{P_LINEA} or $P{P_LINEA} is null)
  AND (ART_GRUPO =$P{P_GRUPO} or $P{P_GRUPO} is null)
  and (AEP_TOT_EXIST <> $P!{p_existencia} or $P{p_existencia} is null)
 Group by ART_MARCA||'-'||MARC_DESC
 ORDER BY ART_MARCA||'-'||MARC_DESC]]>
		</queryString>
		<field name="MARCA_RESUMEN" class="java.lang.String"/>
		<field name="MARCA_CANTIDAD" class="java.math.BigDecimal"/>
		<field name="MARCA_TOT_LOC" class="java.math.BigDecimal"/>
		<field name="MARCA_TOT_MON" class="java.math.BigDecimal"/>
		<variable name="MARCA_TOT_MON_1" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{MARCA_TOT_MON}]]></variableExpression>
