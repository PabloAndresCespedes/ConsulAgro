	<queryString>
		<![CDATA[SELECT DOC_NRO_DOC,
       DOC_CLAVE,
       DOC_CANT_PAGARE,
       W_TOT_COMPROB,
       CUO_IMP_MON,
       CUO_NRO_PAGARE,
       AUX_CUO_NRO_PAGARE,
       PAG_IMP_MON,
       CUO_SALDO_MON,
       TMOV_IND_DBCR_CTA,
       TMOV_ABREV,
       S_NRO_PAGO,
       DOC_FEC_DOC,
       CUO_FEC_VTO,
       PAG_FEC_PAGO,
       S_HIST_VENC,
       S_DIAS_ATRASO,
       S_IMP_INTERES,
       S_PAG_INT,
       DECODE($P{P_OPC_INFORME},'ACUMULADO',S_ACUM_SALDO_MON,CUO_SALDO_MON)S_ACUM_SALDO_MON
  FROM FIN_FINC204_TEMP
 WHERE SESSION_ID = $P{P_SESSION_ID}
   AND EMPR = $P{P_EMPRESA}
ORDER BY TMOV_ABREV,
         DOC_NRO_DOC,
         DOC_FEC_DOC,
         CUO_FEC_VTO,
         PAG_FEC_PAGO]]>
