	<queryString>
		<![CDATA[SELECT T.EMPL_LEGAJO LEGAJO,
       T.NOMBRE NOMBRE,
       T.FECHA,
       T.PCON_DESC,
       T.IMPORTE,
       T.IMPORTE_LOC,
       T.PDOC_MON,
       T.PDDET_CLAVE_FIN,
       EMPL_DOC_IDENT,
       T.FECHA_CAB,
       INITCAP(EXPRESAR_EN_LETRAS.NUMERO_A_LETRAS(IMPORTE)) SALARIO_LETRAS  ,
       TO_CHAR(T.FECHA_CAB, 'YYYY') ANHO,
       null  CLAVE ,
       'Transagro S.A'  EMPRESA ,
       M.MON_ISO MON
  FROM PERL204_TEMP T , GEN_MONEDA  M
  WHERE T.PDOC_MON =  M.MON_CODIGO(+)
  AND T.EMPRESA  = M.MON_EMPR  (+)
       AND T.P_SESSION  = $P!{P_SESSION}
       AND   T.P_USER  = $P{P_LOGIN}
       AND EMPRESA  = $P!{P_EMPRESA}]]>
	</queryString>
