		</parameter>
		<queryString>
			<![CDATA[SELECT TIPO_MONEDA,
       MON_SIMBOLO SIMBOLO,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(A.CHEQ_RECHAZADOS,'999G999G999G999G999')
         ELSE
              TO_CHAR(CHEQ_RECHAZADOS,'999G999G999G990D99')
         END CHEQ_RECHAZADO,
         case
         when TIPO_MONEDA = 1 then
              TO_CHAR(A.CHEQ_DEPOSITAR,'999G999G999G999G999')
         ELSE
              TO_CHAR(A.CHEQ_DEPOSITAR,'999G999G999G990D99')
         END CHEQ_DEPOSITAR,
         case
         when TIPO_MONEDA = 1 then
              TO_CHAR(A.CHEQ_RETENIDOS,'999G999G999G999G999')
         ELSE
              TO_CHAR(A.CHEQ_RETENIDOS,'999G999G999G990D99')
         END CHEQ_RETENIDOS,
         case
         when TIPO_MONEDA = 1 then
              TO_CHAR(A.TRANSFERENCIAS,'999G999G999G999G999')
         ELSE
              TO_CHAR(A.TRANSFERENCIAS,'999G999G999G990D99')
         END TRANSFERENCIAS,
         case
         when TIPO_MONEDA = 1 then
              TO_CHAR(A.CHEQ_REC_ADELANTO,'999G999G999G999G999')
         ELSE
              TO_CHAR(A.CHEQ_REC_ADELANTO,'999G999G999G990D99')
         END CHEQ_REC_ADELANTO,
         TIPO_MONEDA MONEDA
  FROM FIN_FINC004_TEMP_CABECERA A, GEN_MONEDA
   WHERE P_LOGIN = $P{P_LOGIN}
   AND P_SESSION =$P{P_SESSION}
    AND MON_EMPR =$P!{P_EMPRESA}
    AND MON_CODIGO = TIPO_MONEDA]]>
		</queryString>
