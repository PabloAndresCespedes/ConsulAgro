	<subDataset name="MON_CABECERA" uuid="6d2219cd-7a59-4089-b94a-c2a1a8c2e5db">
		<parameter name="P_EMPRESA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_SESSION" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_LOGIN" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT TIPO_MONEDA,
       MON_SIMBOLO SIMBOLO,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(FACTURA_PEND,'999G999G999G999G999')
         ELSE
              TO_CHAR(FACTURA_PEND,'999G999G999G990D99')
         END FACTURA_PEND,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(FACTURA_VENC,'999G999G999G999G999')
         ELSE
              TO_CHAR(FACTURA_VENC,'999G999G999G990D99')
         END FACTURA_VENC,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(PRESTAMO_PEND,'999G999G999G999G999')
         ELSE
              TO_CHAR(PRESTAMO_PEND,'999G999G999G990D99')
         END PRESTAMO_PEND,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(PRESTAMO_VENC,'999G999G999G999G999')
         ELSE
              TO_CHAR(PRESTAMO_VENC,'999G999G999G990D99')
         END PRESTAMO_VENC,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(TOTAL_DEUDA_PEND,'999G999G999G999G999')
         ELSE
              TO_CHAR(TOTAL_DEUDA_PEND,'999G999G999G990D99')
         END TOTAL_DEUDA_PEND,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(TOTAL_DEUDA_VENC,'999G999G999G999G999')
         ELSE
              TO_CHAR(TOTAL_DEUDA_VENC,'999G999G999G990D99')
         END TOTAL_DEUDA_VENC,
       FACTURA_DIAS_ATRASO,
       PRESTAMO_DIAS_ATRASO,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(FACTURA_INTERES,'999G999G999G999G999')
         ELSE
              TO_CHAR(FACTURA_INTERES,'999G999G999G990D99')
         END FACTURA_INTERES,
       case
         when TIPO_MONEDA = 1 then
              TO_CHAR(PRESTAMO_INTERES,'999G999G999G999G999')
         ELSE
              TO_CHAR(PRESTAMO_INTERES,'999G999G999G990D99')
         END PRESTAMO_INTERES,
       TIPO_MONEDA MONEDA
  FROM FIN_FINC004_TEMP_CABECERA, GEN_MONEDA
   WHERE P_LOGIN =$P{P_LOGIN}
   AND P_SESSION =$P{P_SESSION}
    AND MON_EMPR =$P!{P_EMPRESA}
    AND MON_CODIGO = TIPO_MONEDA]]>
		</queryString>
		<field name="TIPO_MONEDA" class="java.math.BigDecimal"/>
		<field name="SIMBOLO" class="java.lang.String"/>
		<field name="FACTURA_PEND" class="java.lang.String"/>
