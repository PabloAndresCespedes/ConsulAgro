	<queryString>
		<![CDATA[SELECT TMOV_ABREV,
       DOC_CLIENTE,
       DOC_NRO_DOC,
       DOC_FEC_DOC,
       CUO_NRO_PAGARE,
       CUO_FEC_VTO,
       S_DIAS_ATRASO,
       CUO_IMP_MON,
       S_NRO_PAGO,
       TIPO_MOV_PAGO,
       PAG_FEC_PAGO,
       PAG_IMP_MON,
       S_PAG_INT,
       S_SALDO_MON_ACUM,
       S_IMP_INTERES,
       TO_CHAR($P{P_IMP_CHEQ_RECHAZADO}, '999G999G999G999G990D00') PAR_RECHAZADO,
       TO_CHAR($P{P_IMP_CHEQ_DIFERIDO}, '999G999G999G999G990D00') PAR_DIFERIDO,
       TO_CHAR($P{P_PREST_SALDO_TENT_MON}, '999G999G999G999G990D00') PAR_PREST_SALDO,
       TO_CHAR($P{P_SALDO_TENT_MON}, '999G999G999G999G990D00') PAR_SALDO_TENT,
       TO_CHAR($P{P_PENDIENTES}, '999G999G999G999G990D00') PAR_PENDIENTES,
       $P{P_VENCIDOS} PAR_VENCIDOS,
       TO_CHAR($P{P_PREST_VENC}, '999G999G999G999G990D00') PAR_PREST_VENC,
       TO_CHAR($P{P_VINTERESES}, '999G999G999G999G990D00') PAR_INT_VAL,
       TO_CHAR($P{P_S_PRESTAMO_VENC}, '999G999G999G999G990D00') PAR_PREST_VEN,
       TO_CHAR($P{P_TRANS_PRENDIENTE}, '999G999G999G999G990D00') PAR_TRANS_PENDIENTE,
       $P{P_IMP_OBS} PAR_IMP_OBS,
       TIPO_MON,
       G.MON_DESC,
       G.MON_SIMBOLO
  FROM FIN_FINC004_TEMP T, GEN_MONEDA G
 WHERE SESSION_ID = $P{P_SESSION_ID}
   AND DOC_CLIENTE IN
       (SELECT CLI_CODIGO
          FROM FIN_CLIENTE
         WHERE CLI_COD_FICHA_HOLDING = $P{P_HOLDING_COD})
   AND T.TIPO_MON = G.MON_CODIGO
   AND G.MON_EMPR = $P{P_EMPRESA}
 ORDER BY T.ROWID ASC
]]>
	</queryString>
