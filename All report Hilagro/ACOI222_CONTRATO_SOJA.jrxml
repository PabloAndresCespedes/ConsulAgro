	<subDataset name="lotes" uuid="5a7685a0-c72a-4259-888c-b988926d6af4">
		<parameter name="P_PERSONA" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{P_PERSONA}]]></defaultValueExpression>
		</parameter>
		<parameter name="P_EMPRESA" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{P_EMPRESA}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT LOTE_PERSONA,
       LOTE_NUMERO,
       LOTE_LOCALIDAD,
       LC.LOC_DESC,
       LOTE_OBS_LOC,
       LOTE_TAM_HAS,
       NVL(LOTE_IND_HIPOTECA, 'N') LOTE_IND_HIPOTEC,
       LOTE_NOM_HIPOTECA,
       LOTE_TIPO_ALQUILER,
       FTP.TIPO_DESC,
       DECODE(LOTE_TIPO_ALQUILER,
              1,
              LOTE_ALQ_KG_HA,
              2,
              LOTE_ALQ_PORC_REND,
              3,
              LOTE_ALQ_PRECIO_HA,
              4,
              LOTE_ALQ_PRECIO_HA,
              NULL) LOTE_VALOR_ALQUILER,
       LOTE_STATUS
  FROM FIN_LOTE_PRODUCTOR LP, GEN_LOCALIDAD LC, FIN_TIPO_ALQ_PROD FTP
 WHERE LOTE_EMPR =  $P{P_EMPRESA}

   AND LOTE_PERSONA =$P{P_PERSONA}

   AND FTP.TIPO_CODIGO(+) = LOTE_TIPO_ALQUILER
   AND TIPO_EMPR(+) = LOTE_EMPR

   AND LC.LOC_CODIGO(+) = LOTE_LOCALIDAD
  AND LC.LOC_EMPR(+) = LOTE_EMPR

ORDER BY 2]]>
		</queryString>
		<field name="LOTE_PERSONA" class="java.math.BigDecimal"/>
		<field name="LOTE_NUMERO" class="java.math.BigDecimal"/>
		<field name="LOTE_LOCALIDAD" class="java.math.BigDecimal"/>
		<field name="LOC_DESC" class="java.lang.String"/>
		<field name="LOTE_OBS_LOC" class="java.lang.String"/>
		<field name="LOTE_TAM_HAS" class="java.math.BigDecimal"/>
		<field name="LOTE_IND_HIPOTEC" class="java.lang.String"/>
		<field name="LOTE_NOM_HIPOTECA" class="java.lang.String"/>
		<field name="LOTE_TIPO_ALQUILER" class="java.math.BigDecimal"/>
		<field name="TIPO_DESC" class="java.lang.String"/>
		<field name="LOTE_VALOR_ALQUILER" class="java.math.BigDecimal"/>
		<field name="LOTE_STATUS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="analisis" uuid="26f132d0-61f6-4ba8-bcd8-63f90ea5a2c9">
		<parameter name="P_EMPRESA" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{P_EMPRESA}]]></defaultValueExpression>
		</parameter>
		<parameter name="P_CLAVE" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{P_CLAVE}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
       F.CAL_DESC,
       F.CAL_CODIGO,
       D.CODET_DCTO,
       D.CODET_DCTO_RANGO,
       CODET_DCTO_REC

  FROM ACO_CONTRATO     A,
       ACO_CONTRATO_DET D,
       ACO_CALIFICACION F
 WHERE

    A.CON_CLAVE = D.CODET_CLAVE_CON(+)
   AND A.CON_EMPR = D.CODET_EMPR
   AND A.CON_EMPR =  $P{P_EMPRESA}
   and a.con_clave = $P{P_CLAVE}


   AND D.CODET_TABLA = F.CAL_CODIGO(+) --

   AND D.CODET_EMPR = F.CAL_EMPR(+)

 ORDER BY F.CAL_CODIGO, D.CODET_DCTO_RANGO]]>
		</queryString>
		<field name="CAL_DESC" class="java.lang.String"/>
		<field name="CAL_CODIGO" class="java.math.BigDecimal"/>
		<field name="CODET_DCTO" class="java.math.BigDecimal"/>
		<field name="CODET_DCTO_RANGO" class="java.lang.String"/>
		<field name="CODET_DCTO_REC" class="java.math.BigDecimal"/>
	</subDataset>
