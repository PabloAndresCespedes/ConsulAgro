	<queryString>
		<![CDATA[SELECT 'J.E.ESTIGARRIBIA ' || TO_CHAR(A.CON_FECHA, 'DD') || ' DE ' ||
       UPPER(LTRIM(RTRIM(TO_CHAR(A.CON_FECHA, 'MONTH', 'nls_date_language=spanish')))) || ' DEL ' ||
       TO_CHAR(A.CON_FECHA, 'YYYY') LUGAR_Y_FECHA,
       TO_CHAR(A.CON_FECHA, 'YYYY') || '/' || A.CON_NRO CON_NRO,

       E.EMPR_RAZON_SOCIAL EMPR_NOMBRE,
       'RUC: ' || E.EMPR_RUC EMPR_RUC_ETI,
       'DIRECCION: ' || E.EMPR_DIR EMPR_DIRECCION_ETI,

       NVL(P.PROV_RAZON_SOCIAL, C.CLI_NOM) PERSONA_NOMBRE,
       'RUC: ' || NVL(P.PROV_RUC, C.CLI_RUC) PERSONA_RUC_ETI,
       'DIRECCION: ' || NVL(P.PROV_DIR, C.CLI_DIR) PERSONA_DIRECCION_ETI,

       S.ART_DESC,
       ----
       DECODE(A.CON_CONDICION,
              'N',
              'A FIJAR',
              LTRIM(TO_CHAR(CON_PREMIO,
                            '999G999G990D00',
                            'NLS_NUMERIC_CHARACTERS = '',.''')) ||
              ' MES BASE: ' || CON_FECHA_EN_CHICAGO) PREMIO,
       ----
       DECODE(A.CON_CONDICION, 'F', TO_CHAR(A.CON_CBOT), 'A FIJAR') CBOT,
       ----
       DECODE(A.CON_CONDICION,
              'F',
              LTRIM(TO_CHAR(A.CON_PRECIO_CHICAGO,
                            '999G999G990D00',
                            'NLS_NUMERIC_CHARACTERS = '',.''')),
              'A FIJAR') PRECIO,
       ----
       A.CON_FECHA_EN_CHICAGO,
       DECODE(A.CON_CONDICION,
              'F',
              'FIJADO',
              'DOS DÍAS HÁBILES ANTERIORES AL FIRST NOTICE DAY (FND) DEL MES BASE CBOT CORRESPONDIENTE.') CON_FECHA_FIJACION,
       ----
       ----
       LTRIM(TO_CHAR(CON_CANTIDAD,
                     '999G999G999',
                     'NLS_NUMERIC_CHARACTERS = '',.''')) CON_CANTIDAD,
       ----
       CON_LOCAL_ENTREGA,
       ----
       CON_PESO_CALIDAD,
       ----
       'DESDE ' || TO_CHAR(CON_PLAZO_ENTREGA_DESDE, 'DD/MM/YYYY') ||
       ' HASTA ' || TO_CHAR(CON_PLAZO_ENTREGA, 'DD/MM/YYYY') CON_PLAZO_ENTREGA,
       ----
       CON_FLETE,


       CON_OBS,
       CON_FORMA_PAGO,
       CON_CONDICION_PAGO,
       CON_MONEDA
       ,con_ind_contrato
       ,con_condicion
,decode(a.con_ind_contrato, 'C', '3 COMPRADOR', '3 VENDEDOR') COM_VEN
,decode(a.con_ind_contrato, 'V', '4 COMPRADOR', '4 VENDEDOR') VEN_COM,
mon_simbolo,
decode(mon_codigo, 1, '/Kg', '/T') unid,  a.con_cbot
  FROM ACO_CONTRATO     A,
       GEN_EMPRESA      E,
       FIN_PROVEEDOR    P,
       STK_ARTICULO     S,
       FIN_CLIENTE      C,
      GEN_MONEDA
 WHERE A.CON_PROVEEDOR = P.PROV_CODIGO(+)
   AND A.CON_EMPR = P.PROV_EMPR(+)
   AND A.CON_CLIENTE = CLI_CODIGO(+)
   AND A.CON_EMPR = CLI_EMPR(+)
   AND A.CON_ARTICULO = S.ART_CODIGO
   AND A.CON_EMPR = S.ART_EMPR
 AND MON_CODIGO = CON_MONEDA
 AND MON_EMPR = CON_EMPR
 AND EMPR_CODIGO = CON_EMPR
   AND A.CON_EMPR =  $P{P_EMPRESA}
   and a.con_clave = $P{P_CLAVE}]]>
	</queryString>
