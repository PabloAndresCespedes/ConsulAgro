	<subDataset name="logo" uuid="e3343c0a-9dd2-4f4b-b34a-3e2c26e98aa8">
		<parameter name="P_EMPRESA" class="java.lang.String"/>
		<queryString>
			<![CDATA[select EMPR_LOGO logo
from gen_empresa
WHERE EMPR_CODIGO =$P{P_EMPRESA}]]>
		</queryString>
		<field name="LOGO" class="java.io.InputStream"/>
	</subDataset>
	<parameter name="P_CLAVE" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_EMPRESA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_IND_COPIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DOC_OBS,
       OCA_DESCUENTO_FAC,
       OCA_KM_KG,
       DOC_MON,
       MON_DESC,
       DOC_CLAVE,
       DOC_NRO_DOC NRO_COMPROBANTE,
       SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
       RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH','nls_date_language=spanish')) || ' de ' ||
       TO_CHAR(DOC_FEC_DOC, 'YYYY') FECHA,
       DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO') COND_VENTA,
       DECODE(DOC_TIPO_MOV, 9, 'X', NULL) MARCA_CONTADO,
       DECODE(DOC_TIPO_MOV, 10, 'X', NULL) MARCA_CREDITO,

       NVL(F.DOC_CLI_RUC, C.CLI_RUC) RUC,
       NVL(F.DOC_CLI_NOM, C.CLI_NOM) NOMBRE,
       NVL(F.DOC_CLI_DIR, C.CLI_DIR) DIRECCION,
       NVL(F.DOC_CLI_TEL, C.CLI_TEL) TELEFONO,
       S.PRO_CODIGO COD_ARTICULO,
       DECODE(D.OCA_IND_EXP_IMP,
              NULL,
                     'SERVICIO DE FLETE DE ' || S.PRO_DESCRIPCION || ' ' ||
                     DOC_OBS,
              'I',
              RPAD('SERVICIO DE FLETE IMPORTACION', 70) || CHR(13) ||
              RPAD('---Observaciones de la factura---', 70) || CHR(13) ||
              DOC_OBS,
              'E',
              RPAD('SERVICIO DE FLETE EXPORTACION', 70) || CHR(13) ||
              RPAD('---Observaciones de la factura---', 70) || CHR(13) ||
              DOC_OBS)

       DESC_ARTICULO,

       NULL ART_COD_BARRA,
       NULL DET_NRO_ITEM,

       DECODE(NVL(OCA_KM_KG, 1), 1, 'KG', 0, 'UN.', 2, 'LTS') ART_UNID_MED,
       NULL PRECIO_UNIT,
       NULL DET_CLAVE_PED,
       SUM(DECODE(NVL(U.OCA_PESO, 0), 0, 1, U.OCA_PESO)) CANTIDAD,
     ---F.DOC_NETO_EXEN_MON EXENTO,
       0 NETO_5,

	nvl(g.det_bruto_mon+g.det_iva_mon,Decode(nvl(F.DOC_NETO_EXEN_MON,0),0,
        DECODE(NVL(OCA_MON, 1),
              1,
              DECODE(NVL(OCA_KM_KG, 1),
                     1,
                     SUM(((ROUND(OCA_PRECIO * 1.1, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) +
                         NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                     2,
                     SUM(((ROUND(OCA_PRECIO * 1.1, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) +
                         NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                     0,
                     SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) * 1.1 +
                         NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT))) -
              SUM(NVL(OCA_DESCUENTO_FAC, 0)),
              DECODE(NVL(OCA_KM_KG, 1),
                     1,
                     SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) +
                         NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                     2,
                     SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) +
                         NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                     SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) +
                         NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                     SUM(NVL(OCA_DESCUENTO_FAC, 0)))),(NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)))) NETO_10,

       F.DOC_IVA_5_MON SUMA_IVA_5,
       F.DOC_IVA_10_MON SUMA_IVA_10,
       (NVL(F.DOC_IVA_5_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)) TOTAL_IVA,
       case DOC_NETO_EXEN_MON
        when 0 then
           0
        else
          DECODE(NVL(OCA_MON, 1),1,
                DECODE(NVL(OCA_KM_KG, 1),--1
                       1,
                       SUM(((ROUND(OCA_PRECIO * 1.1, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) + NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                       2,
                       SUM(((ROUND(OCA_PRECIO * 1.1, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) + NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                       0,
                       SUM(((ROUND(OCA_PRECIO , fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon))* 1.1 + NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT))) - SUM(NVL(OCA_DESCUENTO_FAC, 0)),
                 DECODE(NVL(OCA_KM_KG, 1),--<>1
                       1,
                       SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) + NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                       2,
                       SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon)) + NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                       SUM(((ROUND(OCA_PRECIO, fin_cli_mon_flete(i_empresa =>  $P{P_EMPRESA},
                               i_cliente => oca_cli,
                               i_moneda => oca_mon))  + NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) - SUM(NVL(OCA_DESCUENTO_FAC, 0))
                ))

       end as EXENTO,
       ---F.DOC_NETO_EXEN_MON TOTAL_EXENTO,
       (NVL(F.DOC_GRAV_5_MON, 0) + NVL(F.DOC_IVA_5_MON, 0)) TOTAL_NETO_5,
       (NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)) TOTAL_NETO_10,

       (NVL(F.DOC_NETO_EXEN_MON, 0) + NVL(F.DOC_NETO_GRAV_MON, 0) +
       NVL(F.DOC_IVA_MON, 0)) TOTAL,
       'TIMBRADO N° ' || TIMB_NUMERO TIMB_NUMERO,
       'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_INIC, 'MONTH','nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_INIC, 'YYYY') FEC_TIM_INI,
       'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH','nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_FIN, 'YYYY') FEC_TIM_FIN,
       TIMB_AUTORIZACION,
       MON_DEC_IMP,
       F.DOC_LOGIN,

       NULL DOCU_FAC_CHOFER_NOMBRE,
       NULL DOCU_FAC_VEHICULO_DESC,
       NULL DOCU_FAC_DESTINO_DESC,

       CCO_DESC CCO_HILAGRO_DESC,
       OCA_INTERNACIONAL CONF_DEC_FACT,
       FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                     I_EMPRESA   => DOC_EMPR) CF_VENCIMIENTO,

       FINC001.CCO_CCO_HILAGRO_CFFORMULA(I_CCO_HILAGRO_DESC => CCO_DESC) CCO_CCO_HILAGRO_CF,
       nvl(g.det_precio_mon,(DECODE(NVL(SUM(DECODE(NVL(U.OCA_PESO, 0), 0, 1, U.OCA_PESO)), 0),
              0,
              0,
              NVL(F.DOC_NETO_EXEN_MON, 0) + 0 +
              (DECODE(NVL(OCA_MON, 1),
                      1,
                      DECODE(NVL(OCA_KM_KG, 1),
                             1,
                             SUM(((ROUND(OCA_PRECIO * 1.1, CONF_DEC_FACT) +
                                 NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                             2,
                             SUM(((ROUND(OCA_PRECIO * 1.1, CONF_DEC_FACT) +
                                 NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                             0,
                             SUM(((ROUND(OCA_PRECIO, CONF_DEC_FACT) * 1.1 +
                                 NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT))) -
                      SUM(NVL(OCA_DESCUENTO_FAC, 0)),
                      DECODE(NVL(OCA_KM_KG, 1),
                             1,
                             SUM(((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                 NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                             2,
                             SUM(((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                 NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO),
                             SUM(((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                 NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                             SUM(NVL(OCA_DESCUENTO_FAC, 0)))))))) PRECIO_UNITARIO
  FROM FIN_CCOSTO_HILAGRO,
       FIN_DOCUMENTO F,
       TRA_ORDEN_CARGA D,
       TRA_OCARGA_PROD U,
       TRA_PRODUCTO S,
       FIN_CLIENTE C,
       FIN_PERSONA P,
       FIN_TIMBRADO,
       GEN_MONEDA,
       GEN_TIPO_MOV,
       GEN_SUCURSAL,
       (SELECT DES_EMPR         EMPR,
               DES_CODIGO       CODIGO,
               ORIGEN.LOC_DESC  ORIGEN,
               DESTINO.LOC_DESC DESTINO
          FROM TRA_DESTINOS DES, GEN_LOCALIDAD ORIGEN, GEN_LOCALIDAD DESTINO
         WHERE DES.DES_ORIGEN = ORIGEN.LOC_CODIGO
           AND DES.DES_EMPR = ORIGEN.LOC_EMPR
           AND DES.DES_DESTINO = DESTINO.LOC_CODIGO
           AND DES.DES_EMPR = DESTINO.LOC_EMPR
           AND DES_EMPR = $P{P_EMPRESA}) TRAMO,


       TRA_CONFIGURACION,
       FAC_DOCUMENTO_DET G
 WHERE F.DOC_EMPR = $P{P_EMPRESA}
   AND F.DOC_EMPR = CONF_EMPR

   AND DOC_CLI = PNA_CODIGO
   AND DOC_EMPR = PNA_EMPR

   AND F.DOC_CLAVE = U.OCP_CLAVE_COBRO
   AND F.DOC_EMPR = U.OCP_EMPR

   AND D.OCA_CLAVE = U.OCP_CODIGO
   AND D.OCA_EMPR = U.OCP_EMPR

   AND U.OCP_PRODUCTO = S.PRO_CODIGO
   AND U.OCP_EMPR = S.PRO_EMPR

   AND F.DOC_CLI = C.CLI_CODIGO(+)
   AND F.DOC_EMPR = C.CLI_EMPR(+)

   AND F.DOC_NRO_TIMBRADO = TIMB_NUMERO
   AND F.DOC_EMPR = TIMB_EMPR

   AND G.DET_CLAVE_DOC(+) = F.DOC_CLAVE
   AND G.DET_EMPR(+) = F.DOC_EMPR
    and (g.det_producto = u.ocp_producto or g.det_producto is null)

   AND F.DOC_NRO_DOC BETWEEN TIMB_DOCU_INIC AND TIMB_DOCU_FIN
   AND F.DOC_CLAVE IN ($P!{P_CLAVE})
   AND F.DOC_MON = MON_CODIGO
   AND F.DOC_EMPR = MON_EMPR

   AND F.DOC_TIPO_MOV = TMOV_CODIGO
   AND F.DOC_EMPR = TMOV_EMPR

   AND TMOV_CLASE = TIMB_CLASE_MOV
   AND TMOV_EMPR = TIMB_EMPR

   AND F.DOC_SUC = SUC_CODIGO
   AND F.DOC_EMPR = SUC_EMPR

   AND CCO_CODIGO(+) = DOC_CCO_HILAGRO
   AND CCO_EMPR(+) = DOC_EMPR

   AND D.OCA_DES_CODIGO = TRAMO.CODIGO
   AND D.OCA_EMPR = TRAMO.EMPR

 GROUP BY DOC_MON,
          MON_DESC,
          DOC_CLAVE,
          DOC_NRO_DOC,
          SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
          RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH','nls_date_language=spanish')) || ' de ' ||
          TO_CHAR(DOC_FEC_DOC, 'YYYY'),
          DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO'),
          DECODE(DOC_TIPO_MOV, 9, 'X', NULL),
          DECODE(DOC_TIPO_MOV, 10, 'X', NULL),

          NVL(F.DOC_CLI_RUC, C.CLI_RUC),
          NVL(F.DOC_CLI_NOM, C.CLI_NOM),
          NVL(F.DOC_CLI_DIR, C.CLI_DIR),
          NVL(F.DOC_CLI_TEL, C.CLI_TEL),
          S.PRO_CODIGO,
          S.PRO_DESCRIPCION,

          F.DOC_IVA_5_MON,
          F.DOC_IVA_10_MON,
          (F.DOC_IVA_5_MON + F.DOC_IVA_10_MON),

          F.DOC_NETO_EXEN_MON,
          (NVL(F.DOC_GRAV_5_MON, 0) + NVL(F.DOC_IVA_5_MON, 0)),
          (NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)),

          (NVL(F.DOC_NETO_EXEN_MON, 0) + NVL(F.DOC_NETO_GRAV_MON, 0) +
          NVL(F.DOC_IVA_MON, 0)),
          'TIMBRADO N° ' || TIMB_NUMERO,
          'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_INIC, 'MONTH','nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_INIC, 'YYYY'),
          'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH','nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_FIN, 'YYYY'),
          TIMB_AUTORIZACION,
          MON_DEC_IMP,
          F.DOC_LOGIN,
          OCA_INTERNACIONAL,
          DECODE(NVL(OCA_KM_KG, 1), 1, 'KG', 0, 'UN.'),
          NVL(OCA_KM_KG, 1),
          CCO_DESC,
          OCA_DESCUENTO_FAC,
          DOC_OBS,
          D.OCA_IND_EXP_IMP,
          F.DOC_GRAV_10_MON,
          OCA_KM_KG,
          OCA_DESCUENTO_FAC,
          OCA_MON,
          DECODE(NVL(PNA_MULTINACIONAL, 'N'),
                 'N',
                 'SERVICIO DE FLETE DE ' || S.PRO_DESCRIPCION || DOC_OBS,
                 'SERVICIO DE FLETE DE ' || TRAMO.ORIGEN || '-' ||
                 TRAMO.DESTINO || ' ' || S.PRO_DESCRIPCION),DOC_EMPR,

       DECODE(D.OCA_IND_EXP_IMP,
              NULL,
              DECODE(NVL(PNA_MULTINACIONAL, 'N'),
                     'N',
                     'SERVICIO DE FLETE DE ' || S.PRO_DESCRIPCION || ' ' ||
                     DOC_OBS,
                     'SERVICIO DE FLETE DE ' || TRAMO.ORIGEN || '-' ||
                     TRAMO.DESTINO || ' ' || S.PRO_DESCRIPCION),
              'I',
              RPAD('SERVICIO DE FLETE IMPORTACION', 70) || CHR(13) ||
              RPAD('---Observaciones de la factura---', 70) || CHR(13) ||
              DOC_OBS,
              'E',
              RPAD('SERVICIO DE FLETE EXPORTACION', 70) || CHR(13) ||
              RPAD('---Observaciones de la factura---', 70) || CHR(13) ||
              DOC_OBS),
g.det_bruto_mon, g.det_precio_mon, g.det_iva_mon

 ORDER BY DOC_NRO_DOC]]>
	</queryString>
	<field name="DOC_OBS" class="java.lang.String"/>
	<field name="OCA_DESCUENTO_FAC" class="java.math.BigDecimal"/>
	<field name="OCA_KM_KG" class="java.math.BigDecimal"/>
	<field name="DOC_MON" class="java.math.BigDecimal"/>
	<field name="MON_DESC" class="java.lang.String"/>
	<field name="DOC_CLAVE" class="java.math.BigDecimal"/>
	<field name="NRO_COMPROBANTE" class="java.math.BigDecimal"/>
	<field name="FECHA" class="java.lang.String"/>
	<field name="COND_VENTA" class="java.lang.String"/>
	<field name="MARCA_CONTADO" class="java.lang.String"/>
	<field name="MARCA_CREDITO" class="java.lang.String"/>
	<field name="RUC" class="java.lang.String"/>
