	<queryString>
		<![CDATA[SELECT V.EMPR_CODIGO        ,
       V.EMPR_DESC          ,
       V.PERI_CODIGO        ,
       V.PERI_NOM           ,
       V.TIPO_REV_COD       TIPO_REV_COD   ,
       V.TIPO_REV_DESC      TIPO_REV_DESC  ,
       V.LINEA_COD          ,
       V.LINEA_DESC         ,
       V.LINEA_DESC_L       ,
       V.GRUPO_COD          ,
       V.GRUPO_DESC         ,
       V.GRUPO_DESC_L       ,
       V.REV_PESO_KG        ,
       V.REV_PREC_KG REV_PRECIO_KG      ,
       V.DOCU_FECHA         ,
       --V.DOCU_OPER_COD      ,
       V.DOCU_CLAVE         ,
       V.DOCU_NUMERO        ,
       V.DOCU_FECHA         ,
       V.DOCU_OBS           ,
       V.DETA_ART_COD       ,
       V.DETA_ART_DESC      ,
       V.DETA_EX_ACTUAL     ,
       V.DETA_CANT_CC_ACT   ,
       V.DETA_CU_INICIO     ,
       V.DETA_VALOR_INI     ,
       V.DETA_CU_CIERRE     ,
       V.DETA_VALOR_CIE     ,
       V.DETA_DEPREC        ,
       V.DETA_REVALUO      -- ,
       -- V.DETA_AJUSTE        ,
       -- V.DETA_TIPO_AJ
  FROM STKI091_REP_V V

WHERE   V.EMPR_CODIGO          =      $P{P_EMPRESA}
   AND  V.PERI_CODIGO          =      $P{P_PERI_CODIGO}
   AND  ( V.LINEA_COD           =      $P{P_LIN_CODIGO}
         OR $P{P_LIN_CODIGO}   IS NULL
        )
   AND  ( V.GRUPO_COD           =      $P{P_GRUP_CODIGO}
          OR
          $P{P_GRUP_CODIGO}   IS NULL
        )
]]>
	</queryString>
