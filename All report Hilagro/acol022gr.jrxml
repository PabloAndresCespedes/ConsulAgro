	<parameter name="P_MON_CONT" class="java.lang.String"/>
	<queryString>
		<![CDATA[select doc_zafra,
       prod_codigo,
       doc_producto,
       prod_desc,
       doc_cliente,
       cli_nom,
       doc_operacion,
       doc_fec,
       doc_nro,
       doc_unid_neto,
       doc_precio_unitario,
       doc_precio_unitario_usd,
       doc_importe,
       doc_importe_usd,
       prod_unid_med,
       mon_simbolo,
       operacion_desc,
       unid_debe,
       unid_haber,
       prec_unit,
       importe
from
(select doc_zafra,
       prod_codigo,
       doc_producto,
       prod_desc,
       aco.doc_cliente,
       cli_nom,
       doc_operacion,
       doc_fec,
       prbr_nro_ticket doc_nro,
       doc_unid_neto,
       doc_precio_unitario,
       doc_precio_unitario_usd,
       doc_importe,
       doc_importe_usd,
       prod_unid_med,
       mon_simbolo,

       decode(doc_operacion,'ESP','Salida Prod','LVV','Baja Vtas','VTA','Venta','CRC','Credito','DBC','Debito',' ') operacion_desc,

       case
         when doc_operacion IN ('VTA','LVV','DBC') then
           doc_unid_neto
          else
            0
          end unid_debe,

       case
         when doc_operacion IN ('ESP', 'CRC') then
           doc_unid_neto
          else
            0
          end unid_haber,

         case
         when doc_operacion = 'VTA' then
           case
             when $P{P_MON_CONT} = 'G' then
               doc_precio_unitario
               else
               doc_precio_unitario_usd
               end
        else
             0
        end   prec_unit,

       case
         when doc_operacion = 'VTA' then
           case
             when $P{P_MON_CONT} = 'G' then
               doc_importe
               else
               doc_importe_usd
               end
        else
             0
        end   importe

  from aco_producto,
       fin_cliente,
       aco_documento aco,
       gen_moneda,
       aco_prod_ent_sal
 where prod_clave = doc_producto
   and cli_codigo = aco.doc_cliente
   and doc_ticket_entsal = prbr_clave_ticket
   and doc_operacion in ('ESP')
   and doc_mon = mon_codigo(+)
   AND DOC_EMPR=MON_EMPR(+)
   AND doc_empr=cli_empr
   and doc_empr=prbr_empr
   and prbr_empr=prod_empr

   AND PROD_EMPR   = $P{P_COD_EMPRE}
   AND DOC_ZAFRA   = $P{P_COD_ZAFRA}
   AND PROD_CODIGO = $P{P_COD_PROD}
   AND CLI_CODIGO  = $P{P_COD_PROV}


union all
select doc_zafra,
       prod_codigo,
       doc_producto,
       prod_desc,
       aco.doc_cliente,
       cli_nom,
       doc_operacion,
       doc_fec,
       doc_nro,
       doc_unid_neto,
       doc_precio_unitario,
       doc_precio_unitario_usd,
       doc_importe,
       doc_importe_usd,
       prod_unid_med,
       mon_simbolo,

       decode(doc_operacion,'ESP','Salida Prod','LVV','Baja Vtas','VTA','Venta','CRC','Credito','DBC','Debito',' ') operacion_desc,

       case
         when doc_operacion IN ('VTA','LVV','DBC') then
           doc_unid_neto
          else
            0
          end unid_debe,

       case
         when doc_operacion IN ('ESP', 'CRC') then
           doc_unid_neto
          else
            0
          end unid_haber,

         case
         when doc_operacion = 'VTA' then
           case
             when $P{P_MON_CONT} = 'G' then
               doc_precio_unitario
               else
               doc_precio_unitario_usd
               end
        else
             0
        end   prec_unit,

       case
         when doc_operacion = 'VTA' then
           case
             when $P{P_MON_CONT} = 'G' then
               doc_importe
               else
               doc_importe_usd
               end
        else
             0
        end   importe


  from aco_producto, fin_cliente, aco_documento aco, gen_moneda
 where prod_clave = doc_producto
   and cli_codigo = aco.doc_cliente
   and doc_operacion in ('VTA', 'LVV', 'DBC', 'CRC')
   and doc_mon = mon_codigo(+)
   AND DOC_EMPR=MON_EMPR(+)
   AND doc_empr=cli_empr
   and doc_empr=prod_empr

   AND PROD_EMPR   =  $P{P_COD_EMPRE}
   AND DOC_ZAFRA   =  $P{P_COD_ZAFRA}
   AND PROD_CODIGO = $P{P_COD_PROD}
   AND CLI_CODIGO  = $P{P_COD_PROV})
 order by to_date(doc_fec,'dd-mm-yyyy'),doc_nro]]>
	</queryString>
