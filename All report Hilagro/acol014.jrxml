	</parameter>
	<queryString>
		<![CDATA[SELECT TOT.PROD_CODIGO||' - '||TOT.PROD_DESC Producto,
       TOT.PROD_CODIGO,
       TOT.DOC_PROV,
       TOT.Tot_Recib,
       TOT.Tot_Baja,
       TOT.Tot_Contrato,
       TOT.RAZON_SOCIAL,
       TOT.Tot_Liquidado,
       TOT.Tot_Fijado,

          CASE
           WHEN NVL(TOT.Tot_Fijado,0) IS NOT NULL AND NVL(TOT.Tot_Fijado,0)> 0 THEN
            DECODE( $P{P_MON_CONSOL} , $P{P_MON_US},ROUND(TOT.tot_precio /TOT.Tot_Fijado ,5), $P{P_MON_GS},ROUND(TOT.tot_precio / TOT.Tot_Fijado ,0))
            ELSE 0
           END Precio_Prom,

        CASE
           WHEN NVL(TOT.Tot_Fijado,0) IS NOT NULL AND NVL(TOT.Tot_Fijado,0)> 0 THEN
             CASE
               WHEN $P{P_MON_CONSOL} =  $P{P_MON_US} THEN
                 ROUND(ROUND(TOT.tot_precio,5) / ROUND(TOT.Tot_Fijado,5) ,5) * NVL(ROUND(TOT.Tot_Fijado,5),0)
                 ELSE
                 ROUND(TOT.tot_precio / TOT.Tot_Fijado ,0) * NVL(TOT.Tot_Fijado,0)
              END
             ELSE 0
            END Valor_Fijacion,

          CASE
           WHEN TOT.NETO_RECEP < 0 THEN
             TOT.NETO_RECEP +  TOT.NETO_LIQ -  TOT.NETO_BAJ_VARIOS
             ELSE
             TOT.NETO_RECEP -  TOT.NETO_LIQ -  TOT.NETO_BAJ_VARIOS
           END Totkg_Liquid,

        CASE
          WHEN (TOT.NETO_RECEP -  TOT.cant_fijacion) >= 0 THEN
             TOT.NETO_RECEP -  TOT.cant_fijacion
          ELSE
              0
          END Totkg_Fijar,

         CASE
          WHEN (TOT.NETO_RECEP -  TOT.cant_fijacion) < 0 THEN
            abs(TOT.NETO_RECEP -  TOT.cant_fijacion)
          ELSE
            0
          END TotFijado_Rec,

       TOT.MONEDA,
       TOT.tot_precio,

(select
       case
         when (sum(aco_sub.cant_fijacion)) is not null and (sum(aco_sub.cant_fijacion)) <> 0 then
           case
             when $P{P_MON_CONSOL} = $P{P_MON_GS} then
                (round(sum(aco_sub.tot_precio) / sum(aco_sub.cant_fijacion)) * sum(aco_sub.cant_fijacion))
           else
               round ((round((sum(aco_sub.tot_precio) / sum(aco_sub.cant_fijacion)),5)) * sum(aco_sub.cant_fijacion),5)
           end
       end Valor_Fijacion
from
(select
       prod_codigo,
       con_proveedor doc_prov,
       sum(decode( $P{P_MON_CONSOL},
                  fij_moneda,
                  b.fij_precio,
                  $P{P_MON_GS},
                  b.fij_precio *
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  $P{P_MON_US},
                  b.fij_precio /
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  0) * fij_cantidad)                              tot_precio,
       sum(decode(con_producto, fij_producto, fij_cantidad, 0)) cant_fijacion
  from fin_proveedor a, aco_fijacion b, aco_contrato c, aco_producto
 where con_proveedor = prov_codigo
   AND CON_EMPR=PROV_EMPR
   and con_producto = prod_clave
   AND CON_EMPR=PROD_EMPR
   and con_clave = b.fij_clave_contrato(+)
   AND CON_EMPR =b.fij_EMPR(+)

   AND CON_EMPR =  $P{P_COD_EMPRE}
   AND (CON_ZAFRA = $P{P_COD_ZAFRA} OR  $P{P_COD_ZAFRA} IS NULL)
   AND (PROD_CODIGO =  $P{P_COD_PROD} OR  $P{P_COD_PROD} IS NULL)
   AND (PROV_CODIGO =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)

 group by con_producto,prod_codigo,prod_desc,con_proveedor,prov_razon_social,con_clave,con_cantidad
union all
select
       prod_codigo ,
       fij_proveedor doc_prov,
       sum(decode( $P{P_MON_CONSOL},
                  fij_moneda,
                  b.fij_precio,
                  $P{P_MON_GS},
                  b.fij_precio *
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  $P{P_MON_US},
                  b.fij_precio /
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  0) * fij_cantidad)                              tot_precio,
       sum(fij_cantidad) cant_fijacion
  from fin_proveedor a, aco_fijacion b, aco_producto
 where fij_proveedor = prov_codigo
   AND FIJ_empr=prov_EMPR
   and fij_producto = prod_clave
   AND FIJ_EMPR=PROD_EMPR
   and fij_clave_contrato is null

   AND FIJ_EMPR =  $P{P_COD_EMPRE}
   AND (FIJ_ZAFRA = $P{P_COD_ZAFRA} OR  $P{P_COD_ZAFRA} IS NULL)
   AND (PROD_CODIGO =  $P{P_COD_PROD} OR  $P{P_COD_PROD} IS NULL)
   AND (PROV_CODIGO =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)

 group by fij_producto,prod_codigo,prod_desc,fij_proveedor,prov_razon_social,fij_clave_contrato) aco_sub
 where aco_sub.prod_codigo = TOT.PROD_CODIGO
   AND (aco_sub.doc_prov =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)) ValFijacion


  FROM
(SELECT ACO.PROD_CODIGO,
        ACO.DOC_PROV,

        SUM(CASE
           WHEN ACO.DOC_OPERACION IN ('CON','CRP') THEN
              ACO.DOC_UNID_NETO
           ELSE 0
             END) Tot_Recib,

        SUM(CASE
           WHEN ACO.DOC_OPERACION IN ('LVR','DBP') THEN
             ACO.DOC_UNID_NETO
           ELSE 0
             END) Tot_Baja,

        SUM(ACO.cant_contrato) Tot_Contrato,

        ACO.PROD_DESC,

        ACO.RAZON_SOCIAL,

        SUM(CASE
            WHEN ACO.DOC_OPERACION = 'LCO' THEN
            ACO.DOC_UNID_NETO
            ELSE 0
           END) Tot_Liquidado,

        SUM(ACO.cant_fijacion) Tot_Fijado,

        SUM(CASE
            WHEN (ACO.NETO_RECEP -  ACO.cant_fijacion) >= 0 THEN
              abs(ACO.NETO_RECEP -  ACO.cant_fijacion)
            ELSE
              0
            END) TotFijado_Rec,

       $P{P_MON_CONSOL}   MONEDA,
      SUM(ACO.tot_precio) tot_precio,
      SUM(ACO.NETO_RECEP) NETO_RECEP,
      SUM(ACO.NETO_LIQ) NETO_LIQ,
      SUM(ACO.NETO_BAJ_VARIOS) NETO_BAJ_VARIOS,
      SUM(ACO.cant_fijacion) cant_fijacion
  FROM
(select doc_producto                         DOC_PRODUCTO,
       prod_codigo                           PROD_CODIGO,
       prod_desc                             PROD_DESC,
       aco.doc_prov                          DOC_PROV,
       prov_razon_social                     RAZON_SOCIAL,
       0                                     con_clave,
       doc_operacion                         DOC_OPERACION,
       doc_cli_nom                           DOC_CLI_NOM,
       doc_unid_neto                         DOC_UNID_NETO,
       0                                     cant_contrato,
       0                                     cant_fijacion,
       0                                     tot_precio,

       CASE
           WHEN DOC_OPERACION IN ('CON','CRP') THEN
             doc_unid_neto
           ELSE
             0
           END  NETO_RECEP,

       CASE
           WHEN DOC_OPERACION IN ('LCO') THEN
             doc_unid_neto
           ELSE
             0
           END NETO_LIQ,

       CASE
           WHEN DOC_OPERACION IN ('LVR','DBP') THEN
             doc_unid_neto
           ELSE
             0
           END NETO_BAJ_VARIOS

  from aco_producto, aco_documento aco, fin_proveedor, fin_documento
 where prod_clave = doc_producto
   AND PROD_EMPR=ACO.DOC_EMPR
   and fin_documento.doc_clave(+) = aco.doc_clave_fin
   AND fin_documento.doc_EMPR(+) = aco.doc_EMPR
   and prov_codigo(+) = aco.doc_prov
   AND PROV_EMPR(+) = ACO.DOC_EMPR
   and doc_operacion in ('LCO', 'CON', 'LVR', 'CRP', 'DBP')

   AND ACO.DOC_EMPR=  $P{P_COD_EMPRE}
   AND (TO_DATE(DOC_FEC) >=  TO_DATE($P{P_DESDE},'DD/MM/YYYY') OR  $P{P_DESDE} IS NULL)
   AND (TO_DATE(DOC_FEC) <=  TO_DATE($P{P_HASTA},'DD/MM/YYYY') OR  $P{P_HASTA} IS NULL)
   AND (DOC_ZAFRA = $P{P_COD_ZAFRA} OR  $P{P_COD_ZAFRA} IS NULL)
   AND (PROD_CODIGO =  $P{P_COD_PROD} OR  $P{P_COD_PROD} IS NULL)
   AND (PROV_CODIGO =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)

union all

select con_producto                           DOC_PRODUCTO,
       prod_codigo                            PROD_CODIGO,
       prod_desc                              PROD_DESC,
       con_proveedor                          DOC_PROV,
       prov_razon_social                      RAZON_SOCIAL,
       con_clave                              con_clave,
       'CONTRATO'                             DOC_OPERACION,
       null                                   doc_cli_nom,
       0                                      DOC_UNID_NETO,
       con_cantidad                           cant_contrato,
       sum(decode(con_producto, fij_producto, fij_cantidad, 0)) cant_fijacion,
       sum(decode( $P{P_MON_CONSOL} ,
                  fij_moneda,
                  b.fij_precio,
                   $P{P_MON_GS},
                  b.fij_precio *
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                   $P{P_MON_US},
                  b.fij_precio /
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  0) * decode(con_producto, fij_producto, fij_cantidad, 0)) tot_precio,
        0  NETO_RECEP,
        0  NETO_LIQ,
        0  NETO_BAJ_VARIOS
  from fin_proveedor a, aco_fijacion b, aco_contrato c, aco_producto
 where con_proveedor = prov_codigo
   AND CON_EMPR = PROV_EMPR
   and con_producto = prod_clave
   AND CON_EMPR = PROD_EMPR
   and con_clave = b.fij_clave_contrato(+)
   AND CON_EMPR = b.fij_EMPR(+)

   AND CON_EMPR =  $P{P_COD_EMPRE}
   AND (CON_ZAFRA = $P{P_COD_ZAFRA} OR  $P{P_COD_ZAFRA} IS NULL)
   AND (PROD_CODIGO =  $P{P_COD_PROD} OR  $P{P_COD_PROD} IS NULL)
   AND (PROV_CODIGO =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)

 group by con_producto,
          prod_codigo,
          prod_desc,
          con_proveedor,
          prov_razon_social,
          con_clave,
          con_cantidad
union all
select fij_producto                                    DOC_PRODUCTO,
       prod_codigo                                     PROD_CODIGO,
       prod_desc                                       PROD_DESC,
       fij_proveedor                                   DOC_PROV,
       prov_razon_social                               RAZON_SOCIAL,
       fij_clave_contrato                              con_clave,
       'FIJACION'                                      DOC_OPERACION,
       null                                            doc_cli_nom,
       0                                               DOC_UNID_NETO,
       0                                               cant_contrato,
       sum(fij_cantidad)                               cant_fijacion,
       sum(decode( $P{P_MON_CONSOL},
                  fij_moneda,
                  b.fij_precio,
                  $P{P_MON_GS},
                  b.fij_precio *
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  $P{P_MON_US},
                  b.fij_precio /
                  FIN_BUSCAR_COTIZACION_fec(fij_fec_emis, $P{P_MON_US}, $P{P_COD_EMPRE}),
                  0) * fij_cantidad)                              tot_precio,
        0  NETO_RECEP,
        0  NETO_LIQ,
        0  NETO_BAJ_VARIOS
  from fin_proveedor a, aco_fijacion b, aco_producto
 where fij_proveedor = prov_codigo
   AND FIJ_empr = prov_EMPR
   and fij_producto = prod_clave
   AND FIJ_EMPR = PROD_EMPR
   and fij_clave_contrato is null

   AND FIJ_EMPR =  $P{P_COD_EMPRE}
   AND (FIJ_ZAFRA = $P{P_COD_ZAFRA} OR  $P{P_COD_ZAFRA} IS NULL)
   AND (PROD_CODIGO =  $P{P_COD_PROD} OR  $P{P_COD_PROD} IS NULL)
   AND (PROV_CODIGO =  $P{P_COD_PROV} OR  $P{P_COD_PROV}  IS NULL)

 group by fij_producto,
          prod_codigo,
          prod_desc,
          fij_proveedor,
          prov_razon_social,
          fij_clave_contrato) ACO
group by ACO.PROD_CODIGO,
         ACO.PROD_DESC,
         ACO.PROD_CODIGO,
         ACO.DOC_PROV,
         ACO.RAZON_SOCIAL)TOT
order by TOT.PROD_CODIGO,TOT.RAZON_SOCIAL]]>
