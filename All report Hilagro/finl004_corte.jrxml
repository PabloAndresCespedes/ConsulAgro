	<queryString>
		<![CDATA[SELECT ECC_CLIENTE_DESC CLI_NOM,
       CLI_DIR,
       CLI_RUC,
       ECC_TIPO_DOC     TMOV_ABREV,
       ECC_NRO_DOC DOC_NRO_DOC,
       ECC_MON_SIMBOLO  mON_SIMBOLO,
       ECC_SUC          doc_suc,
       ECC_CLIENTE      doc_cli,
       ECC_FEC_DOC      DOC_FEC_DOC,
       ECC_FEC_VTO      CUO_FEC_VTO,
       ECC_DIAS_ATRASO,
       ECC_TOTAL_DOC TOTAL_DOCUMENTO,
       ECC_PAGO_FEC     PAG_FEC_PAGO,
       ECC_PAGO_IMP  PAG_IMPORTE,
       ECC_PAGO_NRO_DOC DOC_PAGO,
       ECC_DIF_CAMBIO   CUO_DIF_CAMBIO,
       ECC_SALDO        CUO_SALDO,
       ECC_USER,
       ECC_CLAVE_DOC    DOC_CLAVE,
       ECC_DOC_MON      DOC_MON,
       ECC_CUO_IMPORTE  CUO_IMPORTE,
       CASE WHEN  ECC_CLAVE_DOC=LAG(ECC_CLAVE_DOC) OVER(order by ECC_CLIENTE_DESC ASC,
          ECC_CLAVE_DOC ASC,
          ECC_FEC_VTO    ASC) THEN
         NULL
         ELSE
           ECC_SALDO
           END SALDO,
LAG(ECC_CLAVE_DOC) OVER(order by ECC_CLIENTE_DESC ASC,
          ECC_NRO_DOC     ASC,
          ECC_CLAVE_DOC ASC,
          ECC_FEC_VTO    ASC,
          ECC_PAGO_FEC     ASC,
          ECC_PAGO_NRO_DOC  ASC) CLAVE_ANTERIOR,
LAG(ECC_FEC_VTO) OVER(order by ECC_CLIENTE_DESC ASC,
          ECC_CLAVE_DOC ASC,
          ECC_FEC_VTO    ASC) FEC_VTO_AUX
  FROM FIN_EXT_CLIENTE T, FIN_CLIENTE C
 WHERE    T.ECC_CLIENTE = CLI_CODIGO
   AND T.ECC_EMPR = CLI_EMPR
   AND ECC_EMPR = $P{P_EMPRESA}
  AND ECC_USER=$P{P_USUARIO}
   $P!{P_WHERE}
 ORDER BY ECC_CLIENTE_DESC ASC,
          ECC_NRO_DOC     ASC,
          ECC_CLAVE_DOC ASC,
          ECC_FEC_VTO    ASC,
          ECC_PAGO_FEC     ASC,
          ECC_PAGO_NRO_DOC  ASC]]>
	</queryString>
