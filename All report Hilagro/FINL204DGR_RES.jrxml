	<queryString>
		<![CDATA[SELECT PNA_NOMBRE               CLI_NOM,
       PNA_DIRECCION            CLI_DIR,
       PNA_RUC_DV               CLI_RUC,
       NULL                     CLI_OBS,
       DO.DOC_CLI,
       DO.DOC_NRO_DOC,
       DO.DOC_FEC_DOC,
       DO.DOC_CLAVE,
       DO.DOC_SUC,
       DO.DOC_OPERADOR,
       CUO_FEC_VTO,
       DO.DOC_MON,
       DO.DOC_SALDO_LOC,
       DO.DOC_SALDO_MON,
       DO.DOC_SALDO_PER_ACT_LOC,
       DO.DOC_SALDO_PER_ACT_MON,

       NVL(DO.DOC_DIF_CAMBIO, 0) DOC_DIF_CAMBIO,
       NVL(DO.DOC_DIF_CAMBIO_USD, 0) DOC_DIF_CAMBIO_USD,

       DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_IMP_MON, CUO_IMP_MON) CUO_IMP_MON,
       DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_IMP_LOC, CUO_IMP_LOC) CUO_IMP_LOC,
       DECODE(TMOV_IND_DBCR_CTA,
              'D',
              -1 * NVL(PAG_IMP_MON, 0),
              NVL(PAG_IMP_MON, 0)) PAG_IMP_MON,
       DECODE(TMOV_IND_DBCR_CTA,
              'D',
              -1 * NVL(PAG_IMP_LOC, 0),
              NVL(PAG_IMP_LOC, 0)) PAG_IMP_LOC,
       DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_SALDO_MON, CUO_SALDO_MON) CUO_SALDO_MON,
       DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_SALDO_LOC, CUO_SALDO_LOC) CUO_SALDO_LOC,
       DECODE(TMOV_IND_DBCR_CTA,
              'C',
              -1 * CUO_SALDO_PER_ACT_MON,
              CUO_SALDO_PER_ACT_MON) CUO_SALDO_PER_ACT_MON,
       DECODE(TMOV_IND_DBCR_CTA,
              'C',
              -1 * CUO_SALDO_PER_ACT_LOC,
              CUO_SALDO_PER_ACT_LOC) CUO_SALDO_PER_ACT_LOC,

       DECODE(TMOV_IND_DBCR_CTA,
              'C',
              -1 * NVL(CUO_DIF_CAMBIO, 0),
              NVL(CUO_DIF_CAMBIO, 0)) CUO_DIF_CAMBIO,
       DECODE(TMOV_IND_DBCR_CTA,
              'C',
              -1 * NVL(CUO_DIF_CAMBIO_USD, 0),
              NVL(CUO_DIF_CAMBIO_USD, 0)) CUO_DIF_CAMBIO_USD,

       CUO_NRO_PAGARE,
       DO.DOC_CANT_PAGARE          DOC_CANT_PAGARE,
       CLEM_PORC_INTERES_MOROSIDAD CLEM_PORC_INTERES_MOROSIDAD,
       TMOV_IND_DBCR_CTA,
       TMOV_ABREV,
       MON_SIMBOLO,
       DP.DOC_NRO_DOC              DOC_PAGO,
       PAG_FEC_PAGO,
       NVL(clem_porc_interes_morosidad,MON_PORC_INTERES_MOROSIDAD) PORC_INTERES,
       NVL(CONF_DIAS_GRACIA,0) V_ATRASO ,
       CLEM_IMP_LIM_CR             CLEM_IMP_LIM_CR,

    ROUND(DECODE(tmov_ind_dbcr_cta,'D',(CASE WHEN SYSDATE - (CUO_FEC_VTO +  NVL(CONF_DIAS_GRACIA,0) ) > 0 THEN
      SYSDATE - (CUO_FEC_VTO +  NVL(CONF_DIAS_GRACIA,0)) ELSE 0 END),0),0) DIAS_ATRASO,
 DECODE($P{P_MON_SEL},
              DO.DOC_MON,
              DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_IMP_MON, CUO_IMP_MON) ,
              ROUND((DECODE(TMOV_IND_DBCR_CTA, 'C', -1 * CUO_IMP_MON, CUO_IMP_MON) /
              fin_buscar_cotizacion_fec(DO.DOC_FEC_DOC,
                                        $P{P_MON_SEL},
                                         $P{P_EMPRESA})),2)) CUO_IMP,
(select cot_tasa  from stk_cotizacion where cot_fec = DO.DOC_FEC_DOC and rownum = 1) TASA
  FROM FIN_CUOTA,
       FIN_DOCUMENTO     DO,
       FIN_CONFIGURACION,
       FIN_PERSONA,
       GEN_TIPO_MOV,
       GEN_MONEDA,
       FIN_PAGO,
       FIN_DOCUMENTO     DP,
       FIN_CLI_EMPRESA,
       FIN_CLIENTE
 WHERE DO.DOC_EMPR = $P{P_EMPRESA}
   AND DO.DOC_EMPR = CONF_EMPR
   AND PNA_CODIGO = DO.DOC_CLI
   AND PNA_EMPR = DO.DOC_EMPR
   AND TMOV_CODIGO = DO.DOC_TIPO_MOV
   AND TMOV_EMPR = DO.DOC_EMPR
   AND MON_CODIGO = DO.DOC_MON
   AND MON_EMPR = DO.DOC_EMPR
   AND DO.DOC_CLAVE = CUO_CLAVE_DOC
   AND DO.DOC_EMPR = CUO_EMPR
   AND (CUO_CLAVE_DOC = PAG_CLAVE_DOC(+) AND CUO_FEC_VTO = PAG_FEC_VTO(+))
   AND CUO_EMPR = PAG_EMPR(+)
   AND DP.DOC_CLAVE(+) = PAG_CLAVE_PAGO
   AND DP.DOC_EMPR(+) = PAG_EMPR
   AND CLEM_EMPR = DO.DOC_EMPR
   AND CLI_CODIGO = PNA_CODIGO
   AND CLI_EMPR = PNA_EMPR
   AND CLEM_CLI = DO.DOC_CLI
   AND CLEM_EMPR = DO.DOC_EMPR
   AND CLEM_MON = DO.DOC_MON
   AND CLEM_EMPR = DO.DOC_EMPR
   AND TMOV_IND_AFECTA_SALDO = 'C' -- C=CLIENTE, P=PROVEEDOR, N=NO
   AND PNA_CODIGO NOT IN
       (SELECT PNA_CODIGO
          FROM FIN_PERSONA, FIN_PNA_CTA
         WHERE PNA_EMPR =$P{P_EMPRESA}
           AND PNA_CODIGO = FPC_PNA(+)
           AND PNA_EMPR = FPC_EMPR(+)
           AND FPC_OPERADOR = $P{P_OPERADOR})
          $P!{P_WHERE}
ORDER BY PNA_NOMBRE,DO.DOC_CLI,DO.DOC_CLAVE,CUO_FEC_VTO, DP.DOC_FEC_DOC, DO.DOC_FEC_DOC]]>
	</queryString>
