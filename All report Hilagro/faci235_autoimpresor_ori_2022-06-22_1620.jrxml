	<subDataset name="logo" uuid="e3343c0a-9dd2-4f4b-b34a-3e2c26e98aa8">
		<parameter name="P_EMPRESA" class="java.lang.String"/>
		<queryString>
			<![CDATA[select EMPR_LOGO logo
from gen_empresa
WHERE EMPR_CODIGO =$P{P_EMPRESA}]]>
		</queryString>
		<field name="LOGO" class="java.io.InputStream"/>
	</subDataset>
	<parameter name="P_CLAVE" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_EMPRESA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_IND_COPIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DOC_MON,
       DOC_SIST,
       DOC_CLAVE,
       SUBSTR(LPAD(DOC_NRO_DOC,13,'0'), 1,3)
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-9,3 )
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-6,8 )  NRO_COMPROBANTE,
       SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
       RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH', 'nls_date_language=spanish')) ||
       ' de ' || TO_CHAR(DOC_FEC_DOC, 'YYYY') FECHA,
       DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO') COND_VENTA,
       DECODE(DOC_TIPO_MOV, 9, 'X', NULL) MARCA_CONTADO,
       DECODE(DOC_TIPO_MOV, 10, 'X', NULL) MARCA_CREDITO,

       NVL(C.CLI_RUC, F.DOC_CLI_RUC ) RUC,
       NVL(F.DOC_CLI_NOM, C.CLI_NOM) NOMBRE,
       NVL(C.CLI_DIR ,F.DOC_CLI_DIR) DIRECCION,
       NVL(F.DOC_CLI_TEL, C.CLI_TEL) TELEFONO,
       --     D.DET_NRO_ITEM,
       DECODE(F.DOC_LINEA_NEGOCIO,7,S.ART_CODIGO_FABRICA,S.ART_CODIGO) COD_ARTICULO,
       DECODE(DET_DESC_LARGA, NULL, S.ART_DESC, DET_DESC_LARGA) ART_DESC,
       DECODE(F.DOC_LINEA_NEGOCIO,7,ART_DESC,DECODE(DET_DESC_LARGA,NULL,DECODE(DOC_OBS, NULL, S.ART_DESC, ART_DESC || '-' || DOC_OBS),DET_DESC_LARGA))DESC_ARTICULO,
       S.ART_COD_BARRA,
       DET_NRO_ITEM,
       DECODE(S.ART_TIPO, 4, 0, 1) ART_TIPO,
       SUBSTR(S.ART_UNID_MED, 1, 4) ART_UNID_MED,

       (D.DET_BRUTO_MON + D.DET_IVA_MON) / D.DET_CANT PRECIO_UNIT,
       D.DET_CLAVE_PED,
       (SUM(DECODE(NVL(trim(D.DET_CANT), 0), 0, 1, trim(D.DET_CANT)))) CANTIDAD,

       SUM(DECODE(S.ART_IMPU, 1, D.DET_BRUTO_MON, 0)) EXENTO,
      /* ROUND(SUM(DECODE(S.ART_IMPU, 5, (D.DET_BRUTO_MON * 1.05), 0)),DECODE(F.DOC_MON,1,0,2)) NETO_5,
       ROUND(SUM(DECODE(S.ART_IMPU, 2, (D.DET_BRUTO_MON * 1.1), 0)),DECODE(F.DOC_MON,1,0,2)) NETO_10,*/

       ROUND(SUM(DECODE(S.ART_IMPU, 5, (D.DET_BRUTO_MON * 1.05), 0)),2) NETO_5,
       ROUND(SUM(DECODE(S.ART_IMPU, 2, (D.DET_BRUTO_MON * 1.1), 0)),2) NETO_10,

      /*NVL(ROUND(SUM(DECODE(S.ART_IMPU,
              1,
              (D.DET_BRUTO_MON * 1.05),
              2,
              (D.DET_BRUTO_MON * 1.1),
              0)* (D.DET_PORC_DTO / 100)),DECODE(F.DOC_MON, 1, 0, 2)),0) DESCUENTO,*/
       SUM(NVL(DES.DESCUENTO,0)) DESCUENTO,

       F.DOC_IVA_5_MON SUMA_IVA_5,
       F.DOC_IVA_10_MON SUMA_IVA_10,
       (F.DOC_IVA_5_MON + F.DOC_IVA_10_MON) TOTAL_IVA,

       F.DOC_BRUTO_EXEN_MON TOTAL_EXENTO,
      DECODE(DOC_SIST,'ACO',DECODE(S.ART_IMPU, 5,DECODE(DOC_MON,1,D.DET_BRUTO_LOC*1.05,D.DET_BRUTO_MON*1.05),0),SUM(DECODE(S.ART_IMPU, 1, (D.DET_CANT*NVL(D.DET_PRECIO_UNIT,D.DET_PRECIO_MON)), 0)) OVER(PARTITION BY D.DET_CLAVE_DOC)) TOTAL_NETO_5,
       DECODE(DOC_SIST,'ACO',DECODE(S.ART_IMPU, 2,DECODE(DOC_MON,1,D.DET_BRUTO_LOC*1.05,D.DET_BRUTO_MON *1.05),0),SUM(DECODE(S.ART_IMPU, 2, (D.DET_CANT*NVL(D.DET_PRECIO_UNIT,D.DET_PRECIO_MON)), 0)) OVER(PARTITION BY D.DET_CLAVE_DOC)) TOTAL_NETO_10,
       DECODE(DOC_SIST,'ACO',DECODE(DOC_MON,1,round(DET_BRUTO_LOC*1.05),ROUND(D.DET_BRUTO_MON*1.05,2)),SUM(D.DET_CANT*NVL(D.DET_PRECIO_UNIT,D.DET_PRECIO_MON)))  TOTAL,
         'TIMBRADO Nº ' || TIMB_NUMERO TIMB_NUMERO,
       'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_INIC, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_INIC, 'YYYY') FEC_TIM_INI,
       'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_FIN, 'YYYY') FEC_TIM_FIN,
       MON_DEC_IMP,
       F.DOC_LOGIN,
       DOCU_FAC_CHOFER_NOMBRE,
       DOCU_FAC_VEHICULO_DESC,
       DOCU_FAC_DESTINO_DESC,
       DET_LOTE,
       D.DET_OBSERVACION,
       TO_CHAR(F.DOC_FEC_GRAB, 'DD/MM/YYYY HH24:MI:SS') DOC_FEC_GRAB,
       D.DET_PESO_BRUTO,
       D.DET_PESO_TARA,
       DOCU_HA_KM_ACTUAL,
       DOCU_HA_CAMION,
       E.RMAR_VEH_CHAPA RHA_VEH_CHAPA,
       F.DOC_NRO_PCOMPRA_FLOTA,
       decode(D.DET_LOTE, null, ' ', ' Lote :' || DET_LOTE || ';') DET_LOTE_1,
       decode(D.DET_REG_ART, null, ' ', '  Reg. No :' || DET_REG_ART || ';') DET_REG_ART,
       FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                     I_EMPRESA   => DOC_EMPR) CF_VENCIMIENTO,
       (SUM(DECODE(S.ART_IMPU, 1, D.DET_BRUTO_MON, 0)) +
       SUM(DECODE(S.ART_IMPU, 5, (D.DET_BRUTO_MON * 1.05), 0)) +
       SUM(DECODE(S.ART_IMPU, 2, (D.DET_BRUTO_MON * 1.1), 0))) PRECIO_UNITARIO,
       MON_DESC,
       'Autoriz. como Autoimpresor y Timb. Habilitación N° ' ||
       timb_autorizacion timb_autorizacion,
       S.ART_IMPU,
       NULL INDICADOR,
       F.DOC_LOGIN||DECODE(F.DOC_LINEA_NEGOCIO,7,decode(F.DOC_LEGAJO,NULL,NULL,'-'||P.EMPL_NOMBRE||' '||P.EMPL_APE),NULL) EMPL_NOMBRE
 , 'SUCURSAL '||SUC_CODIGO||' : ' SUCU_CODI,
SUC_DIR ||' TEL:'|| SUC_TEL SUCURSAL, SUC_IND_CASA_CENTRAL,
F.DOC_OBS
 FROM FIN_DOCUMENTO        F,
       FAC_DOCUMENTO_DET    D,
       STK_ARTICULO         S,
       FIN_CLIENTE          C,
       FIN_TIMBRADO,
       GEN_MONEDA,
       GEN_TIPO_MOV,
       GEN_SUCURSAL,
       STK_DOCUMENTO W,
       STK_REMI_VEHICULO E,
       PER_EMPLEADO P,
       (SELECT SUM(B.PDET_IMP_DCTO_DET) DESCUENTO , PED_CLAVE , PED_EMPR FROM FAC_PEDIDO A, FAC_PEDIDO_DET B
       WHERE A.PED_CLAVE = B.PDET_CLAVE_PED
         AND A.PED_EMPR = B.PDET_EMPR
         GROUP BY PED_CLAVE , PED_EMPR) DES
 WHERE F.DOC_CLAVE = D.DET_CLAVE_DOC
   AND F.DOC_EMPR = D.DET_EMPR
   AND D.DET_ART = S.ART_CODIGO
   AND D.DET_EMPR = S.ART_EMPR

   AND DES.PED_CLAVE(+) = DOCU_PEDIDO
   AND DES.PED_EMPR(+) = DOCU_EMPR

   AND P.EMPL_LEGAJO(+) = F.DOC_LEGAJO
   AND P.EMPL_EMPRESA(+) = F.DOC_EMPR


   AND F.DOC_CLI = C.CLI_CODIGO(+)
   AND F.DOC_EMPR = C.CLI_EMPR(+)
   AND E.RMAR_VEH_COD(+) = DOCU_HA_CAMION
   AND E.RMAR_EMPR(+) = DOCU_EMPR
   AND NVL(F.DOC_NRO_TIMBRADO,F.DOC_TIMBRADO)  = TIMB_NUMERO
   AND F.DOC_EMPR = TIMB_EMPR
   AND F.DOC_NRO_DOC BETWEEN TIMB_DOCU_INIC AND TIMB_DOCU_FIN
   AND F.DOC_FEC_DOC BETWEEN TIMB_FECH_INIC AND TIMB_FECH_FIN
   AND F.DOC_CLAVE IN ($P!{P_CLAVE})
   AND F.DOC_EMPR = $P{P_EMPRESA}
   AND F.DOC_MON = MON_CODIGO
   AND F.DOC_EMPR = MON_EMPR
   AND F.DOC_TIPO_MOV = TMOV_CODIGO
   AND F.DOC_EMPR = TMOV_EMPR
   AND TMOV_CLASE = TIMB_CLASE_MOV
   AND TMOV_EMPR = TIMB_EMPR
   AND F.DOC_SUC = SUC_CODIGO
   AND F.DOC_EMPR = SUC_EMPR
   AND F.DOC_CLAVE_STK = DOCU_CLAVE(+)
   AND F.DOC_EMPR = DOCU_EMPR(+)
 GROUP BY DOC_MON,
          DOC_SIST,
          DOC_CLAVE,
          DECODE(F.DOC_LINEA_NEGOCIO,7,decode(F.DOC_LEGAJO,NULL,NULL,'-'||P.EMPL_NOMBRE||' '||P.EMPL_APE),NULL),
          timb_autorizacion,
          SUBSTR(LPAD(DOC_NRO_DOC,13,'0'), 1,3)
          ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-9,3 )
          ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-6,8 ) ,
          SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
          RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH', 'nls_date_language=spanish')) ||
          ' de ' || TO_CHAR(DOC_FEC_DOC, 'YYYY'),
          DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO'),
          DECODE(DOC_TIPO_MOV, 9, 'X', NULL),
          DECODE(DOC_TIPO_MOV, 10, 'X', NULL),
          NVL( C.CLI_RUC,F.DOC_CLI_RUC),
          NVL(F.DOC_CLI_NOM, C.CLI_NOM),
          NVL( C.CLI_DIR,F.DOC_CLI_DIR),
          NVL(F.DOC_CLI_TEL, C.CLI_TEL),
          --    D.DET_NRO_ITEM,
          DECODE(F.DOC_LINEA_NEGOCIO,7,S.ART_CODIGO_FABRICA,S.ART_CODIGO),
          S.ART_IMPU,
          DECODE(F.DOC_LINEA_NEGOCIO,7,ART_DESC,DECODE(DET_DESC_LARGA,NULL,DECODE(DOC_OBS, NULL, S.ART_DESC, ART_DESC || '-' || DOC_OBS),DET_DESC_LARGA)),
          DET_NRO_ITEM,
          S.ART_COD_BARRA,
          DECODE(S.ART_TIPO, 4, 0, 1),
          SUBSTR(S.ART_UNID_MED, 1, 4),
          MON_DEC_IMP,
          (D.DET_BRUTO_MON + D.DET_IVA_MON) / D.DET_CANT,
          F.DOC_IVA_5_MON,
          F.DOC_IVA_10_MON,
          (F.DOC_IVA_5_MON + F.DOC_IVA_10_MON),
          F.DOC_BRUTO_EXEN_MON,
          (NVL(F.DOC_GRAV_5_MON, 0) + NVL(F.DOC_IVA_5_MON, 0)),
          (NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)),
          'TIMBRADO Nº ' || TIMB_NUMERO,
          'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_INIC,
                        'MONTH',
                        'nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_INIC, 'YYYY'),
          'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_FIN, 'YYYY'),
          F.DOC_LOGIN,
          D.DET_CLAVE_PED,
          DOCU_FAC_CHOFER_NOMBRE,
          DOCU_FAC_VEHICULO_DESC,
          DOCU_FAC_DESTINO_DESC,
          DET_LOTE,
          D.DET_OBSERVACION,
          F.DOC_FEC_GRAB,
          DECODE(DET_DESC_LARGA, NULL, S.ART_DESC, DET_DESC_LARGA),
          D.DET_PESO_BRUTO,
          D.DET_PESO_TARA,
          DOCU_HA_KM_ACTUAL,
          DOCU_HA_CAMION,
          RMAR_VEH_CHAPA,
          F.DOC_NRO_PCOMPRA_FLOTA,
          decode(D.DET_LOTE, null, ' ', ' Lote :' || DET_LOTE || ';'),
          decode(D.DET_REG_ART,
                 null,
                 ' ',
                 '  Reg. No :' || DET_REG_ART || ';'),
          FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                        I_EMPRESA   => DOC_EMPR),
          MON_DESC,
          D.DET_PRECIO_UNIT,SUC_IND_CASA_CENTRAL,
          D.DET_BRUTO_LOC,D.DET_BRUTO_MON,
          DECODE(S.ART_IMPU, 2, (NVL(D.DET_PRECIO_UNIT,D.DET_PRECIO_MON) * D.DET_CANT),0), D.DET_CLAVE_DOC,DECODE(S.ART_IMPU, 1, (NVL(D.DET_PRECIO_UNIT,D.DET_PRECIO_MON) * D.DET_CANT), 0), 'SUCURSAL '||SUC_CODIGO||' : ',
SUC_DIR ||' TEL:'|| SUC_TEL,
F.DOC_OBS
UNION

SELECT DOC_MON,
       DOC_SIST,
       DOC_CLAVE,
       SUBSTR(LPAD(DOC_NRO_DOC,13,'0'), 1,3)
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-9,3 )
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-6,8 )  NRO_COMPROBANTE,
       SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
       RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH', 'nls_date_language=spanish')) ||
       ' de ' || TO_CHAR(DOC_FEC_DOC, 'YYYY') FECHA,
       DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO') COND_VENTA,
       DECODE(DOC_TIPO_MOV, 9, 'X', NULL) MARCA_CONTADO,
       DECODE(DOC_TIPO_MOV, 10, 'X', NULL) MARCA_CREDITO,

       NVL(C.CLI_RUC,F.DOC_CLI_RUC) RUC,
       NVL(F.DOC_CLI_NOM, C.CLI_NOM) NOMBRE,
       NVL(C.CLI_DIR,F.DOC_CLI_DIR) DIRECCION,
       NVL(F.DOC_CLI_TEL, C.CLI_TEL) TELEFONO,
       --     D.DET_NRO_ITEM,
       TO_CHAR(FC.FCON_CLAVE) COD_ARTICULO,
       NULL ART_DESC,
       DECODE(DCON_OBS, NULL, FCON_DESC, DCON_OBS) DESC_ARTICULO,
       NULL,
       DCON_ITEM,
       NULL,
       NULL ART_UNID_MED,

       (D.DCON_GRAV_MON + D.DCON_IVA_MON),
       NULL DET_CLAVE_PED,
       sum(nvl(dcon_cant_aux, 1)) CANTIDAD,
       F.DOC_BRUTO_EXEN_MON EXENTO,
       SUM(F.DOC_GRAV_5_MON + F.DOC_IVA_5_MON) NETO_5,
       /*DECODE(F.DOC_MON,
              1,
              ROUND(DECODE(D.DCON_PORC_IVA, 5, (D.DCON_GRAV_MON * 1.05), 0),
                    0),
              ROUND(DECODE(D.DCON_PORC_IVA, 5, (D.DCON_GRAV_MON * 1.05), 0),
                    2)) NETO_5,*/
    /*   DECODE(F.DOC_MON,
              1,
              ROUND(DECODE(D.DCON_PORC_IVA, 10, (D.DCON_GRAV_MON * 1.1), 0),
                    0),
              ROUND(DECODE(D.DCON_PORC_IVA, 10, (D.DCON_GRAV_MON * 1.1), 0),
                    3)) NETO_10,*/

 SUM(DOC_GRAV_10_MON + F.DOC_IVA_10_MON) NETO_10,
       0,


       F.DOC_IVA_5_MON SUMA_IVA_5,
       F.DOC_IVA_10_MON SUMA_IVA_10,
       (F.DOC_IVA_5_MON + F.DOC_IVA_10_MON) TOTAL_IVA,

       F.DOC_NETO_EXEN_MON TOTAL_EXENTO,
       (NVL(F.DOC_GRAV_5_MON, 0) + NVL(F.DOC_IVA_5_MON, 0)) TOTAL_NETO_5,
       (NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)) TOTAL_NETO_10,
       SUM(DCON_EXEN_MON+
           DCON_GRAV_MON+
           D.DCON_IVA_MON)TOTAL,
       'TIMBRADO Nº ' || TIMB_NUMERO TIMB_NUMERO,
       'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_INIC, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_INIC, 'YYYY') FEC_TIM_INI,
       'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
       RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
       TO_CHAR(TIMB_FECH_FIN, 'YYYY') FEC_TIM_FIN,
       MON_DEC_IMP,
       F.DOC_LOGIN,

       NULL DOCU_FAC_CHOFER_NOMBRE,
       NULL DOCU_FAC_VEHICULO_DESC,
       NULL DOCU_FAC_DESTINO_DESC,
       NULL DET_LOTE,
       D.DCON_OBS,
       TO_CHAR(F.DOC_FEC_GRAB, 'DD/MM/YYYY HH24:MI:SS'),
       FDD.DET_PESO_BRUTO,
       FDD.DET_PESO_TARA,
       NULL,
       NULL,
       NULL,
       F.DOC_NRO_PCOMPRA_FLOTA,
       decode(FDD.DET_LOTE, null, ' ', ' Lote :' || DET_LOTE || ';') DET_LOTE_1,
       decode(FDD.DET_REG_ART,
              null,
              ' ',
              '  Reg. No :' || DET_REG_ART || ';') DET_REG_ART,
       FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                     I_EMPRESA   => DOC_EMPR) CF_VENCIMIENTO,
       0 PRECIO_UNITARIO,
       MON_DESC,
       'Autoriz. como Autoimpresor y Timb. Habilitación N° ' ||
       timb_autorizacion timb_autorizacion,
       NULL,
       DECODE(FCON_CLAVE,723,'N',914,'N','S') INDICADOR,
       DECODE(F.DOC_LINEA_NEGOCIO,7,decode(F.DOC_LEGAJO,NULL,NULL,'-'||P.EMPL_NOMBRE||' '||P.EMPL_APE),NULL) EMPL_NOMBRE
       , 'SUCURSAL '||SUC_CODIGO||' : ' SUCU_CODI,
SUC_DIR ||' TEL:'|| SUC_TEL SUCURSAL,SUC_IND_CASA_CENTRAL,
F.DOC_OBS
 FROM FIN_DOCUMENTO     F,
       FIN_DOC_CONCEPTO  D,
       FIN_CLIENTE       C,
       FIN_TIMBRADO,
       GEN_MONEDA,
       GEN_TIPO_MOV,
       GEN_SUCURSAL,
       FIN_CONCEPTO      FC,
       FAC_DOCUMENTO_DET FDD,
       PER_EMPLEADO P
 WHERE F.DOC_CLAVE = D.DCON_CLAVE_DOC
   AND F.DOC_EMPR = D.DCON_EMPR
   AND FC.FCON_CLAVE = D.DCON_CLAVE_CONCEPTO
   AND FC.FCON_EMPR = D.DCON_EMPR


   AND P.EMPL_LEGAJO(+) = F.DOC_LEGAJO
   AND P.EMPL_EMPRESA(+) = F.DOC_EMPR

   AND F.DOC_CLI = C.CLI_CODIGO(+)
   AND F.DOC_EMPR = C.CLI_EMPR(+)
   AND NVL(F.DOC_NRO_TIMBRADO,F.DOC_TIMBRADO)  = TIMB_NUMERO (+)
   AND F.DOC_EMPR = TIMB_EMPR (+)
   AND (F.DOC_NRO_DOC > TIMB_DOCU_INIC(+) AND F.DOC_NRO_DOC < TIMB_DOCU_FIN(+) )
   AND (F.DOC_FEC_DOC > TIMB_FECH_INIC(+) AND F.DOC_FEC_DOC < TIMB_FECH_FIN(+) )
   AND F.DOC_CLAVE = FDD.DET_CLAVE_DOC(+)
   AND F.DOC_EMPR = FDD.DET_EMPR(+)
   AND F.DOC_CLAVE IN ($P!{P_CLAVE})
   --AND FCON_CLAVE NOT IN (723, 914)
   AND F.DOC_MON = MON_CODIGO
   AND F.DOC_EMPR = MON_EMPR
   AND F.DOC_TIPO_MOV = TMOV_CODIGO
   AND F.DOC_EMPR = TMOV_EMPR
   AND (TMOV_CLASE = TIMB_CLASE_MOV OR TIMB_CLASE_MOV IS NULL)
   AND (TMOV_EMPR = TIMB_EMPR OR TIMB_EMPR IS NULL)
   AND F.DOC_SUC = SUC_CODIGO
   AND F.DOC_EMPR = SUC_EMPR
   AND F.DOC_CLAVE_STK IS NULL
   AND F.DOC_EMPR = $P{P_EMPRESA}
   AND FDD.DET_ART IS NULL
 GROUP BY DOC_MON,
          DOC_SIST,
          DOC_CLAVE,
          DECODE(F.DOC_LINEA_NEGOCIO,7,decode(F.DOC_LEGAJO,NULL,NULL,'-'||P.EMPL_NOMBRE||' '||P.EMPL_APE),NULL),
          timb_autorizacion,
          SUBSTR(LPAD(DOC_NRO_DOC,13,'0'), 1,3)
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-9,3 )
       ||'-'||SUBSTR(DOC_NRO_DOC, LENGTH(DOC_NRO_DOC)-6,8 ) ,
          SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
          RTRIM(TO_CHAR(DOC_FEC_DOC, 'MONTH', 'nls_date_language=spanish')) ||
          ' de ' || TO_CHAR(DOC_FEC_DOC, 'YYYY'),
          DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO'),
          DECODE(DOC_TIPO_MOV, 9, 'X', NULL),
          DECODE(DOC_TIPO_MOV, 10, 'X', NULL),
          NVL( C.CLI_RUC,F.DOC_CLI_RUC),
          NVL(F.DOC_CLI_NOM, C.CLI_NOM),
          NVL(C.CLI_DIR,F.DOC_CLI_DIR),
          NVL(F.DOC_CLI_TEL, C.CLI_TEL),
          TO_CHAR(FC.FCON_CLAVE),
          DECODE(DCON_OBS, NULL, FCON_DESC, DCON_OBS),

          MON_DEC_IMP,
          F.DOC_IVA_5_MON,
          F.DOC_IVA_10_MON,
          (F.DOC_IVA_5_MON + F.DOC_IVA_10_MON),

          F.DOC_NETO_EXEN_MON,
          (NVL(F.DOC_GRAV_5_MON, 0) + NVL(F.DOC_IVA_5_MON, 0)),
          (NVL(F.DOC_GRAV_10_MON, 0) + NVL(F.DOC_IVA_10_MON, 0)),

          FDD.DET_PRECIO_UNIT * FDD.DET_CANT,
          'TIMBRADO Nº ' || TIMB_NUMERO,
          'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_INIC,
                        'MONTH',
                        'nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_INIC, 'YYYY'),
          'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
          RTRIM(TO_CHAR(TIMB_FECH_FIN, 'MONTH', 'nls_date_language=spanish')) || ' ' ||
          TO_CHAR(TIMB_FECH_FIN, 'YYYY'),
          F.DOC_LOGIN,
          DCON_ITEM,
          (D.DCON_GRAV_MON + D.DCON_IVA_MON),
          F.DOC_BRUTO_EXEN_MON,
          F.DOC_GRAV_5_MON,
          F.DOC_GRAV_10_MON,
          D.DCON_OBS,
          F.DOC_FEC_GRAB,
          FDD.DET_PESO_BRUTO,
          FDD.DET_PESO_TARA,
          DCON_PORC_IVA,
          DCON_GRAV_MON,
          DCON_EXEN_MON,
          F.DOC_NRO_PCOMPRA_FLOTA,
          decode(FDD.DET_LOTE, null, ' ', ' Lote :' || DET_LOTE || ';'),
          decode(FDD.DET_REG_ART,
                 null,
                 ' ',
                 '  Reg. No :' || DET_REG_ART || ';'),
          FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                        I_EMPRESA   => DOC_EMPR),
          MON_DESC,
  DECODE(FCON_CLAVE,723,'N',914,'N','S')
, 'SUCURSAL '||SUC_CODIGO||' : ' ,
SUC_DIR ||' TEL:'|| SUC_TEL
,SUC_IND_CASA_CENTRAL,
F.DOC_OBS
 ORDER BY 3]]>
