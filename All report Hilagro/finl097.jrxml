	<queryString>
		<![CDATA[SELECT NRO_CUMPLIDO_EMB,
       FECHA_CUMPLIDO_EMB,
       DOC_CLAVE CLAVE_FACT,
       CLI_CODIGO,
       CLI_NOM,
       DOC_EMPR,
       DOC_NRO_DOC NRO_FACTURA,
       DOC_FEC_DOC FEC_FACTURA,
       DOC_TIMBRADO NRO_TIMBRADO,
       DET_NRO_ITEM,
       FIN.DET_ART ART_CODIGO,
       FIN.ART_DESC,
      -- CANTIDAD_FACT,
     --  CANTIDAD_KG_FACT,



       CASE
         WHEN ART_TIPO = 4 THEN
          0
         ELSE
          CANTIDAD_FACT
       END CANTIDAD_FACT,
       CASE
         WHEN ART_TIPO = 4 THEN
          0
         ELSE
          CANTIDAD_KG_FACT
       END CANTIDAD_KG_FACT,
       SUM(CASE
             WHEN ART_TIPO = 4 THEN
              0
             ELSE
              DECODE(AFECTA_STOCK, 'N', CANTIDAD, CANTIDAD_FACT)
           END) CANT_REMISION,
       CASE
             WHEN ART_TIPO = 4 THEN
              0
             ELSE

              SUM(DISTINCT DECODE(AFECTA_STOCK, 'N', CANTIDAD_KG_FACT, CANTIDAD_KG))
           END CANT_KG_REMISION,
       --   SUM(DECODE(AFECTA_STOCK, 'N', CANTIDAD, CANTIDAD_FACT)) CANT_REMISION,
       --  NVL(SUM(DECODE(AFECTA_STOCK, 'N', CANTIDAD_KG, CANTIDAD_KG_FACT)), 0) CANT_KG_REMISION,
       IMPORTE_FACT,
       DECODE(AFECTA_STOCK, 'N', DOCU_NRO_DOC, DOC_NRO_DOC) NRO_REMISION,
       DECODE(AFECTA_STOCK, 'N', DOCU_TIMBRADO, DOC_TIMBRADO) NRO_TIBRADO_REM,
       DECODE(AFECTA_STOCK, 'N', DOCU_FEC_EMIS, DOC_FEC_DOC) FEC_REMISION,
      SUM(DECODE(AFECTA_STOCK, 'N', CANTIDAD, CANTIDAD_FACT)) CANT_REMISION,
       --SUM(DECODE(AFECTA_STOCK, 'N', CANTIDAD_KG, CANTIDAD_KG_FACT)) CANT_KG_REMISION,
       IMPORTE_FACT,
         sum(distinct DECODE(AFECTA_STOCK, 'N',IMPORTE_FACT , IMPORTE_REM )) IMPORTE_REM,
       IMPORTE_NCR,
       NRO_PROFORMA
       /*,
       CASE
         WHEN NVL(CANTIDAD_KG_FACT, 0) - NVL(SUM(DECODE(AFECTA_STOCK,'N',CANTIDAD_KG,CANTIDAD_KG_FACT)),0) = 0 THEN
       IMPORTE_FACT
         ELSE
       NVL(SUM(DECODE(AFECTA_STOCK, 'N', IMPORTE_REM, IMPORTE_FACT)),0)
       END IMPORTE_REM*/
  FROM (SELECT DECODE(D.CEE_NRO_CUMPLIDO,
                      NULL,
                      'SIN CUMPLIDO',
                      D.CEE_NRO_CUMPLIDO) NRO_CUMPLIDO_EMB,
               DECODE(D.CEE_FECHA_CUMPLIDO,
                      NULL,
                      'SIN CUMPLIDO',
                      D.CEE_FECHA_CUMPLIDO) FECHA_CUMPLIDO_EMB,
               DOC_CLAVE,
               FP.PRO_NRO NRO_PROFORMA,
               CLI_CODIGO,
               CLI_NOM,
               DOC_EMPR,
               DOC_NRO_DOC,
               DOC_FEC_DOC,
               DOC_TIMBRADO,
               DET_NRO_ITEM,
               DET_ART,
               ART_DESC,
               SUM(DET_CANT) CANTIDAD_FACT,
               SUM(NVL(B.DET_NETO_LOC, 0) + NVL(B.DET_IVA_LOC, 0)) IMPORTE_FACT,
               NVL(NCR_TOTAL, 0) IMPORTE_NCR,
               SUM(DET_CANT) * NVL(c.art_kg_contenido, 1) CANTIDAD_KG_FACT,
               DECODE(DOC_CLAVE_STK, NULL, 'N', 'S') AFECTA_STOCK ,
               ART_TIPO
          FROM FIN_DOCUMENTO A,
               FAC_DOCUMENTO_DET B,
               STK_ARTICULO C,
               (SELECT A.CEE_NRO_CUMPLIDO,
                       A.CEE_FECHA_CUMPLIDO,
                       A.CEE_TASA,
                       A.CEE_CLAVE_FACT,
                       A.CEE_EMPR
                  FROM FIN_CUMP_EMB_EXPORT A
                 WHERE CEE_EMPR = 1
                 GROUP BY A.CEE_NRO_CUMPLIDO,
                          A.CEE_FECHA_CUMPLIDO,
                          A.CEE_TASA,
                          A.CEE_CLAVE_FACT,
                          A.CEE_EMPR) D,
               FIN_CLIENTE E,
               FAC_PROFORMA FP,
               (SELECT DOC_EMPR EMPRESA_ANUL, DOC_CLAVE_PADRE PADRE_ANUL
                                                                           FROM FIN_DOCUMENTO
                                                                          WHERE DOC_EMPR = 1
                                                                          AND DOC_TIPO_MOV IN (47)) ANUL_FAC,
               (SELECT A.DOC_EMPR NCR_CLAVE_EMPR,
                       A.DOC_CLAVE NCR_CLAVE_FACTURA,
                       ROUND(SUM(NVL((E.DOC_NETO_EXEN_LOC + E.DOC_NETO_GRAV_LOC +
                               E.DOC_IVA_LOC),
                               0))  / (SELECT COUNT(DET_NRO_ITEM) FROM FAC_DOCUMENTO_DET WHERE DET_CLAVE_DOC=A.DOC_CLAVE
                               AND DET_EMPR=A.DOC_EMPR  ),0) NCR_TOTAL
                  FROM FIN_DOCUMENTO     A,
                       FIN_UNION_PAGO          B,
                       FIN_DOCUMENTO     C,
                       FIN_UNION_PAGO          D,
                       FIN_DOCUMENTO     E,
                       FIN_DOC_CONCEPTO  F,
                       (SELECT DOC_EMPR EMPRESA_ANUL, DOC_CLAVE_PADRE PADRE_ANUL
                                                                           FROM FIN_DOCUMENTO
                                                                          WHERE DOC_EMPR = 1
                                                                          AND DOC_TIPO_MOV IN (48)) ANULS
                 WHERE A.DOC_CLAVE = B.PAG_CLAVE_DOC
                   AND A.DOC_EMPR = B.PAG_EMPR
                   AND B.PAG_EMPR = C.DOC_EMPR
                   AND B.PAG_CLAVE_PAGO = C.DOC_CLAVE
                   AND A.DOC_IND_EXPORT = 'S'
                   AND A.DOC_CLAVE_STK IS NULL
                   AND C.DOC_CLAVE_PADRE = D.PAG_CLAVE_PAGO
                   AND D.PAG_EMPR = E.DOC_EMPR
                   AND D.PAG_CLAVE_DOC = E.DOC_CLAVE
                   AND E.DOC_CLAVE=DCON_CLAVE_DOC
                   AND E.DOC_EMPR=DCON_EMPR
                   AND DCON_CLAVE_CTACO IN (2650,7,2716,2713)
                   AND E.DOC_TIPO_MOV in(16,46)
                   AND A.DOC_CLAVE = ANULS.PADRE_ANUL(+)
                   AND A.DOC_EMPR = ANULS.EMPRESA_ANUL(+)
                   AND ANULS.PADRE_ANUL IS NULL
                 GROUP BY A.DOC_EMPR, A.DOC_CLAVE) NCR
         WHERE A.DOC_CLAVE = DET_CLAVE_DOC
           AND DOC_EMPR = DET_EMPR
           AND DET_ART = ART_CODIGO
           AND DET_EMPR = ART_EMPR
           AND DOC_CLAVE = D.CEE_CLAVE_FACT(+)
           AND DOC_EMPR = D.CEE_EMPR(+)
           AND DOC_CLI = CLI_CODIGO
           AND DOC_EMPR = CLI_EMPR
           AND DOC_IND_EXPORT = 'S'
           AND DOC_EMPR = NCR_CLAVE_EMPR(+)
           AND DOC_CLAVE = NCR_CLAVE_FACTURA(+)
           AND A.DOC_EXP_PROFORMA=FP.PRO_CLAVE(+)
           AND A.DOC_EMPR=FP.PRO_EMPR(+)
          AND A.DOC_CLAVE = ANUL_FAC.PADRE_ANUL(+)
          AND A.DOC_EMPR = ANUL_FAC.EMPRESA_ANUL(+)
          AND ANUL_FAC.PADRE_ANUL IS NULL
         GROUP BY DOC_CLAVE,
                  DOC_EMPR,
                  DOC_NRO_DOC,
                  DOC_FEC_DOC,
                  DOC_TIMBRADO,
                  DET_ART,
                  ART_DESC,
                  ART_TIPO ,
                  DECODE(DOC_CLAVE_STK, NULL, 'N', 'S'),
                  c.art_kg_contenido,
                  DECODE(D.CEE_NRO_CUMPLIDO,
                         NULL,
                         'SIN CUMPLIDO',
                         D.CEE_NRO_CUMPLIDO),
                  DECODE(D.CEE_FECHA_CUMPLIDO,
                         NULL,
                         'SIN CUMPLIDO',
                         D.CEE_FECHA_CUMPLIDO),
                  CLI_NOM,
                  CLI_CODIGO,
                  NCR_TOTAL,FP.PRO_NRO,DET_NRO_ITEM) FIN,

       (SELECT DOCU_CLAVE_FIN,
               DOCU_CLAVE,
               DOCU_NRO_DOC,
               DOCU_TIMBRADO,
               DOCU_FEC_EMIS,
               DOCU_EMPR,
               DETA_DET_NRO_ITEM,
               DETA_ART,
               ART_DESC,
               SUM(DETA_CANT) CANTIDAD,
               SUM(NVL(DETA_IMP_NETO_LOC, 0) + NVL(DETA_IVA_LOC, 0)) IMPORTE_REM,
               SUM(DETA_CANT) * NVL(c.art_kg_contenido, 1) CANTIDAD_KG
          FROM STK_DOCUMENTO A, STK_DOCUMENTO_DET B, STK_ARTICULO C, (SELECT DOCU_CLAVE_PADRE PADRE_STK, DOCU_EMPR EMPRESA_STK
                                                                          FROM STK_DOCUMENTO D
                                                                         WHERE D.DOCU_CODIGO_OPER in (14,34)
                                                                         AND D.DOCU_EMPR = 1) ANUL_STK
         WHERE A.DOCU_CLAVE = DETA_CLAVE_DOC
           AND DOCU_EMPR = DETA_EMPR
           AND DETA_ART = ART_CODIGO
           AND DETA_EMPR = ART_EMPR
           AND DOCU_CLAVE_FIN IS NOT NULL
           AND A.DOCU_CLAVE = ANUL_STK.PADRE_STK (+)
           AND A.DOCU_EMPR = ANUL_STK.EMPRESA_STK (+)
           AND ANUL_STK.PADRE_STK IS NULL
         GROUP BY DOCU_CLAVE,
                  DOCU_EMPR,
                  DETA_ART,
                  ART_DESC,
                  c.art_kg_contenido,
                  DOCU_NRO_DOC,
                  DOCU_TIMBRADO,
                  DOCU_FEC_EMIS,
                  DOCU_CLAVE_FIN,
                  DETA_DET_NRO_ITEM
        UNION ALL
        SELECT DOCU_CLAVE_FIN,
               DOCU_CLAVE,
               DOCU_NRO_DOC,
               DOCU_TIMBRADO,
               DOCU_FEC_EMIS,
               DOCU_EMPR,
               DET_NRO_ITEM,
               E.PRO_ARTIC_COD DETA_ART,
               NULL,
               SUM(DETA_CANT * NVL(C.ART_KG_CONTENIDO, 1)) CANTIDAD,
               ROUND(SUM(DETA_CANT * NVL(C.ART_KG_CONTENIDO, 1)) *
                     ((F.DET_NETO_LOC) / DET_CANT)) IMPORTE_REM,
               SUM(DETA_CANT * NVL(C.ART_KG_CONTENIDO, 1)) CANTIDAD_KG
          FROM STK_DOCUMENTO     A,
               STK_DOCUMENTO_DET B,
               STK_ARTICULO      C,
               FIN_DOCUMENTO     D,
               FAC_PROFORMA      E,
               FAC_DOCUMENTO_DET F,
                (SELECT DOC_EMPR EMPRESA_ANUL, DOC_CLAVE_PADRE PADRE_ANUL
                                                                           FROM FIN_DOCUMENTO
                                                                          WHERE DOC_EMPR = 1
                                                                          AND DOC_TIPO_MOV IN (47)) ANUL_FAC
         WHERE A.DOCU_CLAVE = DETA_CLAVE_DOC
           AND DOCU_EMPR = DETA_EMPR
           AND DETA_ART = ART_CODIGO
           AND DETA_EMPR = ART_EMPR
           AND DOCU_CLAVE_FIN = DOC_CLAVE
           AND DOCU_EMPR = DOC_EMPR
           AND D.DOC_EXP_PROFORMA = E.PRO_CLAVE
           AND DOC_EMPR = E.PRO_EMPR
           AND DOC_CLAVE = DET_CLAVE_DOC
           AND DOC_EMPR = DET_EMPR
           AND DET_ART = E.PRO_ARTIC_COD
          AND D.DOC_CLAVE = ANUL_FAC.PADRE_ANUL(+)
          AND D.DOC_EMPR = ANUL_FAC.EMPRESA_ANUL(+)
          AND ANUL_FAC.PADRE_ANUL IS NULL
         GROUP BY DOCU_CLAVE,
                  DOCU_EMPR,
                  DOCU_NRO_DOC,
                  E.PRO_FREIGHT_PRICE,
                  DOC_TASA,
                  DOCU_TIMBRADO,
                  DOCU_FEC_EMIS,
                  DOCU_CLAVE_FIN,
                  E.PRO_ARTIC_COD,
                  ((F.DET_NETO_LOC) / DET_CANT), DET_NRO_ITEM) STK
 WHERE DOC_CLAVE = DOCU_CLAVE_FIN(+)
   AND DOC_EMPR = DOCU_EMPR(+)
   AND DET_ART = DETA_ART(+)
   AND DET_NRO_ITEM = DETA_DET_NRO_ITEM(+)

   AND DOC_EMPR =$P{P_EMPRESA}
   AND (DOC_NRO_DOC = $P{P_NRO_FACTURA} OR $P{P_NRO_FACTURA}  IS NULL)
   AND DOC_FEC_DOC BETWEEN  TO_DATE($P{P_FEC_DESDE},'DD/MM/YYYY') AND TO_DATE($P{P_FEC_HASTA},'DD/MM/YYYY')
   AND (DOCU_FEC_EMIS BETWEEN TO_DATE($P{P_FEC_DESDE},'DD/MM/YYYY') AND TO_DATE($P{P_FEC_HASTA},'DD/MM/YYYY') OR   TO_DATE(DOCU_FEC_EMIS,'DD/MM/YYYY') IS NULL)
   AND (CLI_CODIGO = $P{P_COD_CLIENTE} OR $P{P_COD_CLIENTE}  IS NULL)
 GROUP BY NRO_CUMPLIDO_EMB,
          FECHA_CUMPLIDO_EMB,
          DOC_CLAVE,
          CLI_CODIGO,
          CLI_NOM,
          DOC_EMPR,
          DOC_NRO_DOC,
          DOC_FEC_DOC,
          DOC_TIMBRADO,
          FIN.DET_ART,
          FIN.ART_DESC,
          CANTIDAD_FACT,
          CANTIDAD_KG_FACT,
          IMPORTE_FACT,
          DOCU_TIMBRADO,
          ART_TIPO,
          DOCU_NRO_DOC,AFECTA_STOCK,DOCU_FEC_EMIS,
          IMPORTE_NCR,NRO_PROFORMA,DET_NRO_ITEM
  ORDER BY 8 ASC, 5 ASC, 7 ASC, 9 ASC,12 ASC]]>
	</queryString>
