	<queryString>
		<![CDATA[select distinct
       ecc_tipo_doc,
       ecc_nro_doc,
       ecc_mon_simbolo,
       ecc_suc,
       ecc_cliente,
       ecc_cliente_desc,
       to_char(ecc_fec_doc,'DD/MM/YYYY') ecc_fec_doc,
      -- ecc_fec_vto,
       ecc_dias_atraso,
       DECODE(ecc_pago_imp,NULL,ecc_total_doc,0) ecc_total_doc,
       ecc_pago_fec,
       nvl(ecc_pago_imp,0) ecc_pago_imp,
       ecc_pago_nro_doc,
       nvl(ecc_dif_cambio,0) ecc_dif_cambio,
       DECODE(ecc_pago_imp,NULL,ecc_saldo,0) ecc_saldo,
       ecc_user,
       ecc_clave_doc,
       ecc_doc_mon
  from FIN_EXT_CLIENTE t where  (ecc_pago_fec > add_months(TO_DATE('01/10/2020','DD/MM/YYYY'), -2) and
       ecc_pago_fec <=TO_DATE('01/10/2020','DD/MM/YYYY' ) or ecc_saldo > 0)
 Order by ecc_cliente_desc, ecc_cliente]]>
	</queryString>
