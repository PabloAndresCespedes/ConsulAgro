		</parameter>
		<parameter name="P_WHERE" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA[" AND 1=1"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select distinct marca, sum(saldo) salida
  from (select distinct ecc_clave_doc,
                        decode(nvl(art_marca, 0),
                               0,
                               'NO DEFINIDO',
                               (select marc_desc
                                  from stk_marca
                                 where MARC_EMPR = $P{P_EMPRESA}
                                   AND marc_codigo = art_marca)) marca,
                        decode(round(porce, 0),
                               null,
                               ecc_saldo,
                               (porce * ecc_saldo / 100)) saldo

          from FIN_EXT_CLIENTE t, finl004_stk_marca e
         where (ecc_pago_fec > add_months( to_date($P{P_FEC_HASTA},'dd/MM/yyyy'), -2) and
               ecc_pago_fec <= to_date($P{P_FEC_HASTA},'dd/MM/yyyy') or ecc_saldo > 0)
           and ecc_user =$P{P_USUARIO}
           AND ECC_EMPR =$P{P_EMPRESA}
           and ecc_clave_doc = doc_clave(+)
           AND ECC_EMPR = DETA_EMPR(+)
           $P!{P_WHERE}
          )
 group by marca]]>
		</queryString>
		<field name="MARCA" class="java.lang.String"/>
		<field name="SALIDA" class="java.math.BigDecimal"/>
		<variable name="TOTAL_GENERAL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{SALIDA}]]></variableExpression>
		</variable>
