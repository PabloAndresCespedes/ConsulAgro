	<subDataset name="logo" uuid="e3343c0a-9dd2-4f4b-b34a-3e2c26e98aa8">
		<parameter name="P_EMPRESA" class="java.lang.String"/>
		<queryString>
			<![CDATA[select EMPR_LOGO logo
from gen_empresa
WHERE EMPR_CODIGO =$P{P_EMPRESA}]]>
		</queryString>
		<field name="LOGO" class="java.io.InputStream"/>
	</subDataset>
	<parameter name="P_CLAVE" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_EMPRESA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_IND_COPIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT F.DOC_CLAVE,
       F.DOC_MON,
       REPLACE(TO_CHAR(F.DOC_FEC_DOC, 'DD/MM/YYYY'), ' ') FECHA,
       SUBSTR('00' || F.DOC_NRO_DOC, 0, 3) || '-' ||
       SUBSTR(F.DOC_NRO_DOC, 2, 3) || '-' || SUBSTR(F.DOC_NRO_DOC, 5, 7) NRO_COMPROBANTE,
       '(' || F.DOC_PROV || ') ' || NVL(F.DOC_CLI_NOM, C.PROV_RAZON_SOCIAL) NOMBRE,
       NVL(F.DOC_CLI_DIR, C.PROV_DIR) DIRECCION,
       NVL(F.DOC_CLI_RUC, C.PROV_RUC) RUC,
       NVL(F.DOC_CLI_TEL, C.PROV_TEL) TELEFONO,
       NVL(B.DCON_EXEN_LOC, 0) PRECIO_UNIT,
       T.TIMB_NUMERO AS NUMERO_TIMBRADO,
       TO_CHAR(T.TIMB_FECH_INIC, 'DD/MM/YYYY') AS DATE_INI_TIMBRADO,
       TO_CHAR(T.TIMB_FECH_FIN, 'DD/MM/YYYY') AS DATE_FIN_TIMBRADO,
       E.FCON_DESC DESC_ARTICULO,
       (NVL(F.DOC_NETO_EXEN_LOC, 0) + NVL(F.DOC_NETO_GRAV_LOC, 0) +
       NVL(F.DOC_IVA_LOC, 0)) TOTAL,
       1 CANTIDAD,
       M.MON_SIMBOLO,
       T.TIMB_AUTORIZACION,
       M.MON_DESC,
       M.MON_CODIGO,
       DECODE('S','S',SUC_DIR || ' TEL/FAX:' || NVL(SUC_TEL,S.SUC_FAX)||' '||S.SUC_EMAIL,NULL) CENTRAL,
       DECODE(SUC_IND_CASA_CENTRAL,'S',NULL,'SUCURSAL ' || SUC_DESC) SUCU_CODI,
       DECODE(SUC_IND_CASA_CENTRAL,'S',NULL,SUC_DIR || ' TEL/FAX:' || NVL(SUC_TEL,S.SUC_FAX)||' '||S.SUC_EMAIL) SUCURSAL,
       SUC_IND_CASA_CENTRAL
  FROM FIN_DOCUMENTO_COMI015_TEMP    F,
       FIN_TIMBRADO                  T,
       FIN_DOC_CONCEPTO_COMI015_TEMP B,
       FIN_PROVEEDOR                 C,
       FIN_CONCEPTO                  E,
       GEN_MONEDA                    M,
       GEN_SUCURSAL                  S
 WHERE F.DOC_PROV = C.PROV_CODIGO(+)
   AND F.DOC_EMPR = C.PROV_EMPR(+)

   AND F.DOC_SUC = S.SUC_CODIGO
   AND F.DOC_EMPR = S.SUC_EMPR
   AND F.DOC_MON = M.MON_CODIGO
   AND F.DOC_EMPR = M.MON_EMPR

   AND F.DOC_TIMBRADO = T.TIMB_NUMERO(+)
   AND F.DOC_EMPR = TIMB_EMPR(+)

   AND F.DOC_CLAVE = B.DCON_CLAVE_DOC
   AND F.DOC_EMPR = B.DCON_EMPR

   AND F.DOC_NRO_DOC BETWEEN T.TIMB_DOCU_INIC AND T.TIMB_DOCU_FIN
   AND T. TIMB_CLASE_MOV = 8

   AND B.DCON_CLAVE_CONCEPTO = E.FCON_CLAVE
   AND B.DCON_EMPR = E.FCON_EMPR
   AND F.DOC_EMPR = $P{P_EMPRESA}
   AND F.DOC_CLAVE IN ($P!{P_CLAVE})

 ORDER BY 1, 3]]>
	</queryString>
