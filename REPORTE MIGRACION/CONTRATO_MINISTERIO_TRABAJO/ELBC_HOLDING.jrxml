	<queryString>
		<![CDATA[SELECT x.CONT_REP_NOMBRE,
       x.CONT_EMPR_DESC,
       x.CONT_REP_EDAD,
       x.CONT_REP_OFICIO,
       x.CONT_REP_NAC,
       x.CONT_REP_DOMICILIO,
       x.CONT_EMPL_DIST,
       x.CONT_EMPL_NOMBRE,
       x.CONT_EMPL_EDAD,
       DECODE(x.CONT_EMPL_SEXO, 'F', 'femenino', 'M', 'masculino') SEXO,
       DECODE(x.CONT_EMPL_EST_CIVIL,
              'S',
              'soltero/a',
              'C',
              'casado/o',
              'D',
              'separado/a',
              'V',
              'viudo/a') EST_CIVIL,
       x.CONT_EMPL_OFICIO,
       x.CONT_EMPL_NAC,
       x.CONT_EMPL_DOMICILIO,
       x.CONT_PRIM_SERVICIO,
       x.CONT_PRIM_LUGAR,
       x.CONT_SEG_A,
       x.CONT_SEG_B,
       x.CONT_SEG_C,
       x.CONT_SEG_D,
       x.CONT_TER_A,
       x.CONT_TER_B,
       x.CONT_TER_C,
       x.CONT_TER_D,
       x.CONT_CUA_A,
       x.CONT_CUA_B,
       x.CONT_CUA_B1,
       x.CONT_SEXT_A,
       x.CONT_SEXT_B,
       x.CONT_SEXT_C,
       x.CONT_SEXT_DIV,
       x.CONT_SEXT_A1,
       x.CONT_SEXT_A2,
       x.CONT_SEXT_B1,
       x.CONT_SEXT_B2,
       x.CONT_SEXT_E1,
       x.CONT_SEXT_E2,
       x.CONT_SEXT_F,
       x.CONT_SEP_A,
       x.CONT_SEP_A1,
       x.CONT_SEP_B,
       x.CONT_SEP_B1,
       x.CONT_SEP_C,
       x.CONT_SEP_C1,
       x.CONT_OCT_A,
       x.CONT_OCT_B,
       x.CONT_NOV_A,
       x.CONT_NOV_B,
       x.CONT_NOV_C,
       x.CONT_FIRM_CONF,
       x.CONT_CLAVE,
       x.CONT_EMPR,
       x.CONT_COD_POST,
       x.CONT_COD_EMPL,
       x.CONT_EMPL_AREA,
       x.CONT_EMPL_INGRESO,
       x.CONT_EMPL_PAIS,
       x.CONT_EMPL_PROF,
       x.CONT_CARGO,
       initcap(p.conf_cont_geren_dir) P_DIR_GEREN,
       initcap(p.conf_cont_direc)     P_DIR_LUGAR,
       p.conf_genero_geren            P_GEN_GEREN,
       p.conf_estado_civ_geren        P_EST_CIV_GEREN,
       p.conf_nacionalidad_geren      P_NAC_GEREN
  FROM PER_CONTRATO_MTESS x
  inner join per_configuracion p on (p.conf_empr = x.cont_empr)
 WHERE x.CONT_EMPR = $P{P_EMPRESA}
AND x.cont_clave=$P{P_CLAVE}]]>
	</queryString>
