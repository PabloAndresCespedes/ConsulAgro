	<subDataset name="logo" uuid="e3343c0a-9dd2-4f4b-b34a-3e2c26e98aa8">
		<parameter name="P_EMPRESA" class="java.lang.String"/>
		<queryString>
			<![CDATA[select EMPR_LOGO logo
from gen_empresa
WHERE EMPR_CODIGO =$P{P_EMPRESA}]]>
		</queryString>
		<field name="LOGO" class="java.io.InputStream"/>
	</subDataset>
	<parameter name="P_CLAVE" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_EMPRESA" class="java.lang.String">
		<defaultValueExpression><![CDATA[2]]></defaultValueExpression>
	</parameter>
	<parameter name="P_IND_COPIA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT /*+ INDEX(FIN_DOCUMENTO XAK_FIN_DOC_EMPR_CLI_TM_ND) */
       DOC_OBS,
       OCA_DESCUENTO_FAC,
       OCA_KM_KG,
       DOC_MON,
       MON_DESC,
       DOC_CLAVE,
       NRO_COMPROBANTE,
       FECHA,
       COND_VENTA,
       MARCA_CONTADO,
       MARCA_CREDITO,
       RUC,
       NOMBRE,
       DIRECCION,
       TELEFONO,
       COD_ARTICULO,
       DESC_ARTICULO,
       ART_COD_BARRA,
       DET_NRO_ITEM,
       ART_UNID_MED,
       PRECIO_UNIT,
       DET_CLAVE_PED,
       SUM(CANTIDAD) CANTIDAD,
       SUM(NETO_5) NETO_5,
       SUM(NETO_10) NETO_10,
       SUMA_IVA_5,
       SUMA_IVA_10,
       TOTAL_IVA,
       SUM(EXENTO) EXENTO,
       TOTAL_NETO_5,
       TOTAL_NETO_10,
       TOTAL,
       TIMB_NUMERO,
       FEC_TIM_INI,
       FEC_TIM_FIN,
       TIMB_AUTORIZACION,
       MON_DEC_IMP,
       DOC_LOGIN,
       DOCU_FAC_CHOFER_NOMBRE,
       DOCU_FAC_VEHICULO_DESC,
       DOCU_FAC_DESTINO_DESC,
       CCO_HILAGRO_DESC,
       CONF_DEC_FACT,
       CF_VENCIMIENTO,
       CCO_CCO_HILAGRO_CF,
       PRECIO_UNITARIO,
       DECODE(NVL(CLEM_CANT_REDONDEO, 0), 0, '#,##0', '#,##0.0000') REDONDEO,
       SUCU_CODI,
       SUC_IND_CASA_CENTRAL,
SUCURSAL
  FROM (SELECT DOC_OBS,
               OCA_DESCUENTO_FAC,
               OCA_KM_KG,
               DOC_MON,
               MON_DESC,
               DOC_CLAVE,
               '00' || SUBSTR(DOC_NRO_DOC, 1, 1) || '-' ||
               SUBSTR(DOC_NRO_DOC, 2, 3) || '-' ||
               SUBSTR(DOC_NRO_DOC, 5, 11) NRO_COMPROBANTE,
               SUC_LOCALIDAD || ', ' || TO_CHAR(DOC_FEC_DOC, 'DD') || ' de ' ||
               RTRIM(TO_CHAR(DOC_FEC_DOC,
                             'MONTH',
                             'nls_date_language=spanish')) || ' de ' ||
               TO_CHAR(DOC_FEC_DOC, 'YYYY') FECHA,
               DECODE(DOC_TIPO_MOV, 9, 'CONTADO', 'CREDITO') COND_VENTA,
               DECODE(DOC_TIPO_MOV, 9, 'X', NULL) MARCA_CONTADO,
               DECODE(DOC_TIPO_MOV, 10, 'X', NULL) MARCA_CREDITO,

               NVL(FIN_DOCUMENTO.DOC_CLI_RUC, FIN_PERSONA.PNA_RUC_DV||' - '||PNA_DV) RUC,
               NVL(FIN_DOCUMENTO.DOC_CLI_NOM, FIN_PERSONA.PNA_RAZON_SOCIAL) NOMBRE,
               NVL(FIN_DOCUMENTO.DOC_CLI_DIR, FIN_PERSONA.PNA_DIRECCION) DIRECCION,
               NVL(FIN_DOCUMENTO.DOC_CLI_TEL, FIN_PERSONA.PNA_TELEFONO) TELEFONO,
               TRA_PRODUCTO.PRO_CODIGO COD_ARTICULO,
              DECODE(TRA_ORDEN_CARGA.OCA_IND_EXP_IMP,
                      NULL,
                      DECODE(FAC_DOCUMENTO_DET.DET_PRODUCTO,
                             NULL,
                             FAC_DOCUMENTO_DET.DET_DESC_LARGA,
                             'SERVICIO DE FLETE DE ' || TRA_PRODUCTO.PRO_DESCRIPCION || ' ' ||
                             DET_OBSERVACION),
                      'I',
                      RPAD('SERVICIO DE FLETE IMPORTACION', 70) || CHR(13) ||
                      RPAD('---Observaciones de la factura---', 70) ||
                      CHR(13) || DET_OBSERVACION,
                      'E',
                      RPAD('SERVICIO DE FLETE EXPORTACION', 70) || CHR(13) ||
                      RPAD('---Observaciones de la factura---', 70) ||
                      CHR(13) || DET_OBSERVACION)

               DESC_ARTICULO,

               NULL ART_COD_BARRA,
               NULL DET_NRO_ITEM,

               DECODE(NVL(OCA_KM_KG, 1), 1, 'KG', 0, 'UN.', 2, 'LTS') ART_UNID_MED,
               NULL PRECIO_UNIT,
               NULL DET_CLAVE_PED,
               DECODE(FAC_DOCUMENTO_DET.DET_PRODUCTO,
                      NULL,
                      FAC_DOCUMENTO_DET.DET_CANT,
                      DECODE(NVL(TRA_OCARGA_PROD.OCA_PESO, TRA_OCARGA_PROD.OCP_CANT),
                             0,
                             1,
                             NVL(TRA_OCARGA_PROD.OCA_PESO, TRA_OCARGA_PROD.OCP_CANT))) CANTIDAD,
               ---F.DOC_NETO_EXEN_MON EXENTO,
               0 NETO_5,

               NVL(FAC_DOCUMENTO_DET.DET_BRUTO_MON + FAC_DOCUMENTO_DET.DET_IVA_MON,
                   DECODE(NVL(FIN_DOCUMENTO.DOC_NETO_EXEN_MON, 0),
                          0,
                          DECODE(NVL(OCA_MON, 1),
                                 1,
                                 DECODE(NVL(OCA_KM_KG, 1),
                                        1,
                                        ((ROUND(OCA_PRECIO * 1.1,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) +
                                        NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                        2,
                                        ((ROUND(OCA_PRECIO * 1.1,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) +
                                        NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                        0,
                                        ((ROUND(OCA_PRECIO,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) * 1.1 +
                                        NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                                 NVL(OCA_DESCUENTO_FAC, 0),
                                 DECODE(NVL(OCA_KM_KG, 1),
                                        1,
                                        ((ROUND(OCA_PRECIO,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) +
                                        NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                        2,
                                        ((ROUND(OCA_PRECIO,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) +
                                        NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                        ((ROUND(OCA_PRECIO,
                                                FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                  I_CLIENTE => OCA_CLI,
                                                                  I_MONEDA  => OCA_MON)) +
                                        NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT) -
                                        NVL(OCA_DESCUENTO_FAC, 0))),
                          (NVL(FIN_DOCUMENTO.DOC_GRAV_10_MON, 0) +
                          NVL(FIN_DOCUMENTO.DOC_IVA_10_MON, 0)))) NETO_10,

               FIN_DOCUMENTO.DOC_IVA_5_MON SUMA_IVA_5,
               FIN_DOCUMENTO.DOC_IVA_10_MON SUMA_IVA_10,
               (NVL(FIN_DOCUMENTO.DOC_IVA_5_MON, 0) + NVL(FIN_DOCUMENTO.DOC_IVA_10_MON, 0)) TOTAL_IVA,
               CASE DOC_NETO_EXEN_MON
                 WHEN 0 THEN
                  0
                 ELSE
                  DECODE(NVL(OCA_MON, 1),
                         1,
                         DECODE(NVL(OCA_KM_KG, 1), --1
                                1,
                                ((ROUND(OCA_PRECIO * 1.1,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) +
                                NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                2,
                                ((ROUND(OCA_PRECIO * 1.1,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) +
                                NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                0,
                                ((ROUND(OCA_PRECIO,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) * 1.1 +
                                NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                         NVL(OCA_DESCUENTO_FAC, 0),
                         DECODE(NVL(OCA_KM_KG, 1), --<>1
                                1,
                                ((ROUND(OCA_PRECIO,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) +
                                NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                2,
                                ((ROUND(OCA_PRECIO,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) +
                                NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                ((ROUND(OCA_PRECIO,
                                        FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                          I_CLIENTE => OCA_CLI,
                                                          I_MONEDA  => OCA_MON)) +
                                NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT) -
                                NVL(OCA_DESCUENTO_FAC, 0))) -
                  NVL(FAC_DOCUMENTO_DET.DET_BRUTO_MON + FAC_DOCUMENTO_DET.DET_IVA_MON,
                      DECODE(NVL(FIN_DOCUMENTO.DOC_NETO_EXEN_MON, 0),
                             0,
                             DECODE(NVL(OCA_MON, 1),
                                    1,
                                    DECODE(NVL(OCA_KM_KG, 1),
                                           1,
                                           ((ROUND(OCA_PRECIO * 1.1,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) +
                                           NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                           2,
                                           ((ROUND(OCA_PRECIO * 1.1,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) +
                                           NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                           0,
                                           ((ROUND(OCA_PRECIO,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) * 1.1 +
                                           NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                                    NVL(OCA_DESCUENTO_FAC, 0),
                                    DECODE(NVL(OCA_KM_KG, 1),
                                           1,
                                           ((ROUND(OCA_PRECIO,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) +
                                           NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                           2,
                                           ((ROUND(OCA_PRECIO,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) +
                                           NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                           ((ROUND(OCA_PRECIO,
                                                   FIN_CLI_MON_FLETE(I_EMPRESA => 2,
                                                                     I_CLIENTE => OCA_CLI,
                                                                     I_MONEDA  => OCA_MON)) +
                                           NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT) -
                                           NVL(OCA_DESCUENTO_FAC, 0))),
                             (NVL(FIN_DOCUMENTO.DOC_GRAV_10_MON, 0) +
                             NVL(FIN_DOCUMENTO.DOC_IVA_10_MON, 0))))
               END EXENTO,

               ---F.DOC_NETO_EXEN_MON TOTAL_EXENTO,
               (NVL(FIN_DOCUMENTO.DOC_GRAV_5_MON, 0) + NVL(FIN_DOCUMENTO.DOC_IVA_5_MON, 0)) TOTAL_NETO_5,
               (NVL(FIN_DOCUMENTO.DOC_GRAV_10_MON, 0) + NVL(FIN_DOCUMENTO.DOC_IVA_10_MON, 0)) TOTAL_NETO_10,

               (NVL(FIN_DOCUMENTO.DOC_NETO_EXEN_MON, 0) + NVL(FIN_DOCUMENTO.DOC_NETO_GRAV_MON, 0) +
               NVL(FIN_DOCUMENTO.DOC_IVA_MON, 0)) TOTAL,
               'TIMBRADO N° ' || TIMB_NUMERO TIMB_NUMERO,
               'Fecha Inicio Vigencia ' || TO_CHAR(TIMB_FECH_INIC, 'DD') || ' ' ||
               RTRIM(TO_CHAR(TIMB_FECH_INIC,
                             'MONTH',
                             'nls_date_language=spanish')) || ' ' ||
               TO_CHAR(TIMB_FECH_INIC, 'YYYY') FEC_TIM_INI,
               'Fecha Fin Vigencia ' || TO_CHAR(TIMB_FECH_FIN, 'DD') || ' ' ||
               RTRIM(TO_CHAR(TIMB_FECH_FIN,
                             'MONTH',
                             'nls_date_language=spanish')) || ' ' ||
               TO_CHAR(TIMB_FECH_FIN, 'YYYY') FEC_TIM_FIN,
               TIMB_AUTORIZACION,
               MON_DEC_IMP,
               FIN_DOCUMENTO.DOC_LOGIN,

               NULL DOCU_FAC_CHOFER_NOMBRE,
               NULL DOCU_FAC_VEHICULO_DESC,
               NULL DOCU_FAC_DESTINO_DESC,

               CCO_DESC CCO_HILAGRO_DESC,
               OCA_INTERNACIONAL CONF_DEC_FACT,
               FINC001.CF_VENCIMIENTOFORMULA(I_DOC_CLAVE => DOC_CLAVE,
                                             I_EMPRESA   => DOC_EMPR) CF_VENCIMIENTO,

               FINC001.CCO_CCO_HILAGRO_CFFORMULA(I_CCO_HILAGRO_DESC => CCO_DESC) CCO_CCO_HILAGRO_CF,
               NVL(FAC_DOCUMENTO_DET.DET_PRECIO_MON,
                   (DECODE(NVL(DECODE(NVL(TRA_OCARGA_PROD.OCA_PESO, 0), 0, 1, TRA_OCARGA_PROD.OCA_PESO),
                               0),
                           0,
                           0,
                           NVL(FIN_DOCUMENTO.DOC_NETO_EXEN_MON, 0) + 0 +
                           (DECODE(NVL(OCA_MON, 1),
                                   1,
                                   DECODE(NVL(OCA_KM_KG, 1),
                                          1,
                                          ((ROUND(OCA_PRECIO * 1.1,
                                                  CONF_DEC_FACT) +
                                          NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                          2,
                                          ((ROUND(OCA_PRECIO * 1.1,
                                                  CONF_DEC_FACT) +
                                          NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                          0,
                                          ((ROUND(OCA_PRECIO, CONF_DEC_FACT) * 1.1 +
                                          NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT)) -
                                   NVL(OCA_DESCUENTO_FAC, 0),
                                   DECODE(NVL(OCA_KM_KG, 1),
                                          1,
                                          ((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                          NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                          2,
                                          ((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                          NVL(OCA_VALOR_AGREG, 0))) * OCA_PESO,
                                          ((ROUND(OCA_PRECIO, CONF_DEC_FACT) +
                                          NVL(OCA_VALOR_AGREG, 0)) * OCP_CANT) -
                                          NVL(OCA_DESCUENTO_FAC, 0))))))) PRECIO_UNITARIO,
               FIN_CLI_EMPRESA.CLEM_CANT_REDONDEO,
               'SUCURSAL '||SUC_CODIGO||' : ' SUCU_CODI,
               SUC_DIR ||' TEL:'|| SUC_TEL SUCURSAL, SUC_IND_CASA_CENTRAL

          FROM FIN_CCOSTO_HILAGRO FIN_CCOSTO_HILAGRO,
               FIN_DOCUMENTO FIN_DOCUMENTO,
               TRA_ORDEN_CARGA TRA_ORDEN_CARGA,
               TRA_OCARGA_PROD TRA_OCARGA_PROD,
               TRA_PRODUCTO TRA_PRODUCTO,
               FIN_PERSONA FIN_PERSONA,
               FIN_TIMBRADO FIN_TIMBRADO,
               GEN_MONEDA GEN_MONEDA,
               GEN_SUCURSAL GEN_SUCURSAL,
               (SELECT DES_EMPR         EMPR,
                       DES_CODIGO       CODIGO,
                       ORIGEN.LOC_DESC  ORIGEN,
                       DESTINO.LOC_DESC DESTINO
                  FROM TRA_DESTINOS  DES,
                       GEN_LOCALIDAD ORIGEN,
                       GEN_LOCALIDAD DESTINO
                 WHERE DES.DES_ORIGEN = ORIGEN.LOC_CODIGO
                   AND DES.DES_EMPR = ORIGEN.LOC_EMPR
                   AND DES.DES_DESTINO = DESTINO.LOC_CODIGO
                   AND DES.DES_EMPR = DESTINO.LOC_EMPR
                   AND DES_EMPR = 2) TRAMO,

               TRA_CONFIGURACION TRA_CONFIGURACION,
               FAC_DOCUMENTO_DET FAC_DOCUMENTO_DET,
               FIN_CLI_EMPRESA   FIN_CLI_EMPRESA
         WHERE FIN_DOCUMENTO.DOC_EMPR = $P{P_EMPRESA}
           AND FIN_DOCUMENTO.DOC_CLAVE IN($P!{P_CLAVE})
           AND FIN_DOCUMENTO.DOC_EMPR = TRA_CONFIGURACION.CONF_EMPR

           AND FIN_CLI_EMPRESA.CLEM_EMPR = FIN_PERSONA.PNA_EMPR
           AND FIN_CLI_EMPRESA.CLEM_CLI = FIN_PERSONA.PNA_CODIGO
           AND FIN_CLI_EMPRESA.CLEM_MON = FIN_DOCUMENTO.DOC_MON

           AND FIN_DOCUMENTO.DOC_CLI = FIN_PERSONA.PNA_CODIGO
           AND FIN_DOCUMENTO.DOC_EMPR = FIN_PERSONA.PNA_EMPR

           AND FIN_DOCUMENTO.DOC_CLAVE = TRA_OCARGA_PROD.OCP_CLAVE_COBRO
           AND FIN_DOCUMENTO.DOC_EMPR = TRA_OCARGA_PROD.OCP_EMPR

           AND TRA_ORDEN_CARGA.OCA_CLAVE = TRA_OCARGA_PROD.OCP_CODIGO
           AND TRA_ORDEN_CARGA.OCA_EMPR = TRA_OCARGA_PROD.OCP_EMPR

           AND TRA_OCARGA_PROD.OCP_PRODUCTO = TRA_PRODUCTO.PRO_CODIGO
           AND TRA_OCARGA_PROD.OCP_EMPR = TRA_PRODUCTO.PRO_EMPR


           AND FIN_DOCUMENTO.DOC_NRO_TIMBRADO = FIN_TIMBRADO.TIMB_NUMERO
           AND FIN_DOCUMENTO.DOC_EMPR = FIN_TIMBRADO.TIMB_EMPR
           AND FIN_DOCUMENTO.DOC_NRO_DOC BETWEEN FIN_TIMBRADO.TIMB_DOCU_INIC AND FIN_TIMBRADO.TIMB_DOCU_FIN
           AND FIN_TIMBRADO.TIMB_CLASE_MOV = 1

           AND FAC_DOCUMENTO_DET.DET_CLAVE_DOC(+) = FIN_DOCUMENTO.DOC_CLAVE
           AND FAC_DOCUMENTO_DET.DET_EMPR(+) = FIN_DOCUMENTO.DOC_EMPR


           AND NVL(FAC_DOCUMENTO_DET.DET_OC, TRA_OCARGA_PROD.OCP_CODIGO) = TRA_OCARGA_PROD.OCP_CODIGO
